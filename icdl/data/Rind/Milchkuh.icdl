					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "kein intakter Schwanz"
					long-description = "Vorliegen von mindestens einer der folgenden Veränderungen: Verletzung (Wunde/Kruste), (alter) Bruch,verkürzte Schwänze (abgerissen oder amputiert)."
					images = [ ]
					thumbnail = ""
				} //End-Var labelClass1
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Schwanzschäden"
