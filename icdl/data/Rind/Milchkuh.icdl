} // End-Indicator indicatorLabel

Indicator "Klauenzustand" {
	last-modified = "timestamp der letzten Änderung"
	name = "Klauenzustand"
	description = "Mängel an den Klauen lassen Rückschlüsse auf den Pflegezustand der Tiere zu und können gesundheiltiche Schäden und Leistungsrückgänge nach sich ziehen."
	animal-category = "Milchkuh"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Der Anteil an Milchkühen mit Mängeln an den Klauen lässt Rückschlüsse auf den Pflegezustand der Tiere zu. Ein mangelhafter Klauenzustand liegt z. B. bei übermäßiger Klauenlänge und damit häufig verbundener Krümmung der Klauenspitze nach innen oder Verletzungen und Überwachsungen des Wandhorns vor. Er kann Fehlstellungen und damit eine unphysiologische Gewichtsverteilung, ein verändertes Gangbild und Beeinträchtigungen des Verhaltens (z. B. Futteraufnahme) zur Folge haben. Zudem beeinflusst er das Wohlbefinden und kann Klauenerkrankungen begünstigen. In der Folge davon kann die Milchleistung vermindert sein. Der Zustand der Klauen ergibt sich aus dem Zusammenwirken von verschiedenen Faktoren wie Klauenpflegemanagement, Umweltbedingungen und individueller, genetisch bedingter Hornqualität. Risikofaktoren für mangelhaften Klauenzustand sind zu seltene oder nicht fachgerechte Klauenpflege, Bewegungsmangel, ungünstige Bodenbeschaffenheit (mangelnder Abrieb, nicht trittsicher, feucht, verschmutzt) und nicht wiederkäuergerechte Fütterung (z. B. Pansenazidose)."
	when-and-how-often = "Halbjährlich, zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten"
	which-and-how-many = "Stichprobe, die alle Gruppen laktierender Kühe sowie die trockenstehenden Kühe einer Herde anteilig berücksichtigt"
	how = "Es wird je Tier eine Körperseite zufällig ausgewählt. Aus maximal 2 m Entfernung werden die zwei Klauenpaare (vorderes und hinteres Klauenpaar) der zugewandten Körperseite beurteilt. Erfasst werden zu lange Klauen, gebogene Klauen, Klauen mit unvollständigem Bodenkontakt, Wandläsionen und unregelmäßige Wandoberflächen. Der Klauenzustand ist mangelhaft, wenn mindestens einer der genannten Mängel an mindestens einem Klauenpaar auftritt. "
	branches = ["Milchviehhaltung"]
	sorting = 10
	Assessment {
		default_type = "By_Animal"        
		type_changeable = false 
		presentation = "classification"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "Keine Mängel an beiden Klauenpaaren"
					long-description = "Beschreibung der ersten Option"
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Mängel an mindestens einem Klauenpaar"
					long-description = <<EOF
Vorliegen von mindestens einem der folgenden Mängel an mindestens einem Klauenpaar: 
Kriterien: 
Zu lang/Klauenspitze evtl. gebogen
Kein voller Bodenkontakt 
Wandläsion 
Unregelmäßige Wandoberfläche 
EOF
					images = [ ]
					thumbnail = ""
				} //End-Var labelClass1
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Klauenzustand"
			description = "Anteil an Tieren in der Stichprobe mir mangelhaftem Klauenzustand"
			expression= "(A / (A + B)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 20
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Mängel an den Klauen."
					lowerbound = 0 
					upperbound = 5 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Mängel an den Klauen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 15
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "Lahmheit, Anbindehaltung" {
	last-modified = "timestamp der letzten Änderung"
	name = "Lahmheit"
	description = "Lahmheiten sind eines der wichtigsten Tierschutzprobleme und ziehen gsundheitliche und wirtschaftliche Probleme nach sich."
	animal-category = "Milchkuh"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Der Begriff Lahmheit beschreibt Störungen des Gangbildes, die bei Milchkühen überwiegend schmerzbedingt sind. Aufgrund des häufigen Auftretens werden Lahmheiten daher als eines der wichtigsten Tierschutzprobleme in der Milchviehhaltung angesehen. Lahme Kühe sind aber nicht nur Schmerzen ausgesetzt, sondern haben auch eingeschränkten Zugang zu Futter und Wasser oder anderen Ressourcen. In der Folge sind Milchleistung und Fruchtbarkeit bei lahmen Tieren geringer. Lahmheit geht bei Milchkühen überwiegend von Klauenerkrankungen oder Infektionen der Haut am Unterfuß aus; zusätzlich können Gelenksveränderungen eine Rolle spielen. Die wichtigsten Risikofaktoren sind der Liegebereich (Überbelegung; harte Liegefläche; unzureichende Abmessungen), die Beschaffenheit der Laufflächen (rutschig, uneben, verschmutzt, feucht), Stoffwechselstörungen aufgrund von Fütterungsfehlern (Pansenübersäuerung und Eiweißüberschuss; Mineralstoffmangel) sowie das Fehlen einer regelmäßigen funktionellen Klauenpflege."
	when-and-how-often = "Halbjährlich, zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten"
	which-and-how-many = "Stichprobe, die alle Gruppen laktierender Kühe sowie die trockenstehenden Kühe einer Herde anteilig berücksichtigt"
	how = "Lahmheiten werden durch Beurteilung des Ganges auf einem befestigten Untergrund erfasst. Dabei wird in gering- und hochgradige Lahmheiten unterschieden. Bei Anbindehaltung ohne Auslauf, Weidegang oder Melkstand erfolgt ersatzweise eine Beurteilung von Lahmheitsanzeichen im Stand. Bei Laufstallhaltung Beurteilung der Lahmheit durch Gangbeobachtung."
	branches = ["Milchviehhaltung"]
	sorting = 10
	Assessment {
		Sample { // work-in-progress
			type = "complete|partial"
			partial-factor = 0.3 // This key will only be of use if the sample-type is "partial"
		} 
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 2: Anbindehaltung, Beurteilung von Anzeichen von Lahmheiten im Stand"
				Var "D" {
					name = "Boniturnote 0"
					short-description = "nicht lahm"
					long-description = "Keine Lahmheitsanzeichen erkennbar "
					images = [ ]
				}
				Var "E" {
					name = "Boniturnote 1"
					short-description = "lahm"
					long-description = <<EOF
Lahm, falls eines der drei folgenden Kriterien zutrifft:
Wiederholtes Anheben oder Entlasten eines Beins
Aufsetzen des vorderen Klauenteils auf die Kante einer Stufe oder Leisten eines Gitterrosts
Deutliche Entlastung einer Gliedmaße bei der seitlichen Bewegung im Stand 
EOF
					images = [ ]
					thumbnail = ""
				}
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Anbindehaltung: Tiere mit Lahmheitsanzeichen"
			description = "Anteil an Tieren in der Stichprobe, die bei Anbindehaltung lahm sind"
			expression= "(D / (D + E)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 10
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier geht lahm."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 3 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere gehen lahm, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 8
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}		
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

