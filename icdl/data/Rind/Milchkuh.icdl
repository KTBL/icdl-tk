				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere liegen unvollständig in der Liegefläche, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 1
					upperbound = 2
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

