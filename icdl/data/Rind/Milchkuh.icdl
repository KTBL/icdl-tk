					thumbnail = ""
				} 
				Var "B" {
					name = "unvollständig"
					short-description = "Kühe liegen unvollständig auf der Liegefläche bzw. in der Liegebox"
					long-description = "Kühe liegen unvollständig auf der Liegefläche bzw. in der Liegebox: Tiere werden als unvollständig liegend gezählt, wenn sich mehr als die Hälfte der gedachten Verbindungslinie zwischen Sitzbeinhöcker und Hüfthöcker außerhalb der Liegefläche befindet."
					images = [ ]
					thumbnail = ""
				} 
				Var "C" {
					name = "nicht mit liegen beschäftigt"
					short-description = "Kühe sind aktiv mit der Futter- oder Wasseraufnahme beschäftigt"
					long-description = "Kühe sind aktiv mit der Futter- oder Wasseraufnahme beschäftigt"
					images = [ ]
				} 
				Var "D" {
					name = "stehend"
					short-description = "Kühe stehen mit zwei oder vier Beinen in der Box"
					long-description = "Kühe stehen mit zwei oder vier Beinen in der Box"
					images = [ ]
					thumbnail = ""
