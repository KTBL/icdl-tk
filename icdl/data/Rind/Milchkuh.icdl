				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere, die nicht aktiv mit der Futter- oder Wasseraufnahme beschäftig sind, liegen nicht in der Herde, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0
					upperbound = 50
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			}
