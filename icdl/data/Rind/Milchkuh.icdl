				Var "E" {
					name = "Spaltenlieger"
					short-description = "Kühe liegen vollständig außerhalb der Liegefläche („Spaltenlieger“)"
					long-description = "Kühe liegen vollständig außerhalb der Liegefläche („Spaltenlieger“)"
					images = [ ]
					thumbnail = ""
				} 
				Var "F" {
					name = "stehen oder gehen"
					short-description = "Kühe, die weder aktiv mit der Futter- oder Wasseraufnahme beschäftigt sind, noch liegen oder in den Liegeboxen stehen"
					long-description = "Kühe, die weder aktiv mit der Futter- oder Wasseraufnahme beschäftigt sind, noch liegen oder in den Liegeboxen stehen"
					images = [ ]	
				} //End-Var labelClass4
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Stall Usage Index - Laufstallhaltung"
			description = "Liegende Tiere bezogen auf alle Tiere der Gruppe, die nicht mit der Futter- oder Wasseraufnahme beschäftigt sind"
