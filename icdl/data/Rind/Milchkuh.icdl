Indicator "Verschmutzung der Tiere" {
	last-modified = "timestamp der letzten Änderung"
	name = "Verschmutzung der Tiere"
	description = "Verschmutzungen der Tiere können zu gesundheitlichen Problemen führen und können auf Managementprobleme hinweisen."
	animal-category = "Milchkuh"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Verschmutzungen des Haarkleids können Juckreiz hervorrufen, die darunterliegende Haut kann sich entzünden und die Thermoregulation ist beeinträchtigt. Verschmutzungen zeigen Schwachstellen in Haltung und Management auf. Kotverschmutzungen der Unterbeine sowie des Euters und des Brust-/Bauchbereichs weisen auf nicht oder nur schlecht geräumte Laufflächen hin, deren Schmutz durch die Klauen auf die Liegeflächen getragen wird. Stärkere Verfärbungen, große Flecken und sich auf der Hinterhand aufbauende Kotanhaftungen spiegeln ein unzureichendes Management der Liegeflächen oder aber auch falsch eingesetzte bzw. fehlende Einstreu wider. Weitere Risikofaktoren sind zu dünner Kot durch mangelhafte Rationsgestaltung oder kotverschmutzte Triebwege, Weide- und Auslaufflächen."
	when-and-how-often = "Halbjährlich, zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten"
	which-and-how-many = "Stichprobe, die alle Gruppen laktierender Kühe sowie die trockenstehenden Kühe einer Herde anteilig berücksichtigt"
	how = "Es wird je Tier eine Körperseite zufällig ausgewählt. Aus maximal 2 m Entfernung werden die Körperregionen „Unteres Hinterbein“ (vom Kronsaum bis inkl. des Sprunggelenks; Außen- und Innenseite; dazu kann auch die Innenseite des gegenüberliegenden Beines betrachtet werden.), „Oberes Hinterbein“ (oberhalb des Sprunggelenks, inkl. Schwanz, ohne Schwanzquaste, ohne Euter) und „Euter“ (inkl. des halben Euterspiegels der zu beurteilenden Seite von hinten) getrennt beurteilt."
	branches = ["Milchviehhaltung"]
	sorting = 10
	Assessment {
		default_type = "By_Animal"        
		type_changeable = false 
		presentation = "classification"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Unteres Hinterbein"
				Var "A" {
					name = "sauber"
					short-description = "sauber"
					long-description = "Keine Verschmutzung bzw. nur nasses Fell oder Verfärbung ohne Kotauflagerung"
					images = [ ]
					thumbnail = ""
				}
				Var "B" {
					name = "verschmutzt"
					short-description = "verschmutztes unteres Hinterbein"
					long-description = "Je Körperregion in der Summe mindestens handtellergroße Kotauflagerungen"
					images = [ ]
					thumbnail = ""
				}
			}
			Group {
				name = "oberes Hinterbein"
				Var "C" {
					name = "sauber"
					short-description = "sauber"
					long-description = "Keine Verschmutzung bzw. nur nasses Fell oder Verfärbung ohne Kotauflagerung"
					images = [ ]
					thumbnail = ""
				}
				Var "D" {
					name = "Verschmutzt"
					short-description = "verschmutztes oberes Hinterbein"
					long-description = "Je Körperregion in der Summe mindestens handtellergroße Kotauflagerungen"
					images = [ ]
					thumbnail = ""
				}
			}
			Group {
				name = "Euter"
				Var "E" {
					name = "sauber"
					short-description = "sauber"
					long-description = "Keine Verschmutzung bzw. nur nasses Fell oder Verfärbung ohne Kotauflagerung"
					images = [ ]
					thumbnail = ""
				}
				Var "F" {
					name = "verschmutzt"
					short-description = "verschmutztes Euter"
					long-description = "Je Körperregion in der Summe mindestens handtellergroße Kotauflagerungen"
					images = [ ]
					thumbnail = ""
				}
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
