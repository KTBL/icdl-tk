			// Options is only used if the type is "classification"
			Group {
				name = "Schwanzgrube"
				Var "A" {
					name = "zu mager"
					short-description = "Tiefe Grube unter dem Schwanzansatz"
					long-description = "Tiefe Grube unter dem Schwanzansatz"
					images = [
						"Koerperkondition1",
						"Koerperkondition2"]
					thumbail = [
						"Koerperkondition1"]
				}
				Var "B" {
					name = "normal"
					short-description = "Schwanzgrube noch vorhanden, Schwanzansatz hebt sich als leichte Kuppe ab"
					long-description = "Schwanzgrube noch vorhanden, Schwanzansatz hebt sich als leichte Kuppe ab"
					images = [
						"Koerperkondition3",
						"Koerperkondition4"]
					thumbnail = ""
				}
				Var "C" {
					name = "zu fett"
					short-description = "Schwanzgrube ausgefüllt, Faltenbildung"
					long-description = "Schwanzgrube ausgefüllt, Faltenbildung"
					images = [
						"Koerperkondition5",
						"Koerperkondition6"]
					thumbnail = ""
				}
				//End-Var labelClass2
			}
			// End-Group Bonituren Gruppe 1
			Group {
				name = "Lendenbereich"
				Var "D" {
					name = "zu mager"
					short-description = "Tiefe Einbuchtung zwischen Wirbelsäule und Hüfthöcker"
					long-description = "Tiefe Einbuchtung zwischen Wirbelsäule und Hüfthöcker"
					images = [
						"Koerperkondition7",
						"Koerperkondition8"]
					thumbnail = ""
				}
				Var "E" {
					name = "normal"
					short-description = "Leichte Einbuchtung (Milchrasse) bzw. gerade Linie (Zweinutzungsrasse) zwischen Wirbelsäule und Hüfthöcker"
					long-description = "Leichte Einbuchtung (Milchrasse) bzw. gerade Linie (Zweinutzungsrasse) zwischen Wirbelsäule und Hüfthöcker"
					images = [
						"Koerperkondition9",
						"Koerperkondition10"]
					thumbnail = ""
				}
				Var "F" {
					name = "zu fett"
					short-description = "Keine Einbuchtung (Milchrasse) bzw. Aufwölbung der Linie zwischen Wirbelsäule und Hüfthöcker"
					long-description = "Keine Einbuchtung (Milchrasse) bzw. Aufwölbung der Linie zwischen Wirbelsäule und Hüfthöcker"
					images = [
						"Koerperkondition11",
						"Koerperkondition12"]
					thumbnail = ""
				}
				//End-Var labelClass2
			}
			// End-Group Bonituren Gruppe 1
			Group {
				name = "Querfortsätze"
				Var "G" {
					name = "zu mager"
					short-description = "Scharfe Enden der Querfortsätze"
					long-description = "Scharfe Enden der Querfortsätze"
