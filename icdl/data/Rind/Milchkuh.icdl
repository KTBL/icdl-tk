				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben verschmutzte untere Hinterbeine, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 55
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			}
		}
		Formula {
			name = "Tiere mit verschmutzem oberen Hinterbein"
			description = "Anteil Tiere mit verschmutztem oberem Hinterbein "
			expression= "(C / (C+D)) * 100"
