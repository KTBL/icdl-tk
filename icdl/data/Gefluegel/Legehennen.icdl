Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Verletzt oder tot angelieferte Tiere"
	description = "Die Zahl der verletzt oder tot angelieferten Tiere wird beeinflusst durch Erkrankungen und Vorschädigungen im Bestand, unsachgemäße Behandlung der Tiere beim Fangen und Verbringen in die Transportbehältnisse sowie beim Entladen, zu hohe oder zu niedrige Besatzdichten in den Behältnissen, Stress durch hohe Temperaturen und Luftfeuchte während des Verladens und Transports, die Fahrweise beim Transport und zu lange Wartezeiten am Schlachthof."
	animal-category = "Legehennen"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Verletzt oder tot am Schlachthof angelieferte Legehennen haben in der Regel starke Beeinträchtigungen des Wohlergehens durch die Verletzungen oder bis zum Eintritt des Todes erfahren. Die Zahl der verletzt oder tot angelieferten Tiere wird beeinflusst durch Erkrankungen und Vorschädigungen im Bestand, unsachgemäße Behandlung der Tiere beim Fangen und Verbringen in die Transportbehältnisse sowie beim Entladen, zu hohe oder zu niedrige Besatzdichten in den Behältnissen, Stress durch hohe Temperaturen und Luftfeuchte während des Verladens und Transports (hier auch zu niedrige Temperaturen), die Fahrweise beim Transport und zu lange Wartezeiten am Schlachthof. Bei auffällig vielen tot angelieferten Tieren sollte die Höhe der Mortalität während der Legephase mit betrachtet werden. Ein mögliches Krankheits-/Seuchengeschehen muss als potenzielle Ursache berücksichtigt werden. Frische Brüche von Schenkeln, Flügeln und Brustbein sowie Hämatome, frische Verletzungen und Kratzer sind in der Regel durch Fangen, Verladen und/oder Transport verursacht, während ältere Verletzungen und verknöcherte Brustbeinbrüche schon während der Haltungsperiode entstanden sind. Viele Schlachthöfe melden die durch Fangen, Verladen und Transport verletzten Tiere nicht separat zurück, daher kann dieser Teilindikator unter Umständen nicht ausgewertet werden."
	when-and-how-often = "Nach Ende des Durchgangs Schlachthofrückmeldungen auswerten."
	which-and-how-many = "Gesamtherde"
	how = "Auswertung der Schlachthofrückmeldung bezüglich A) Anteil durch Fangen, Verladen und Transport verletzt angelieferter Tiere (z. B. Brüche von Schenkeln und Flügeln, Hämatome, teils auch unter Bezeichnung „Fangfehler“ oder „Fangschäden“ aufgelistet) und B) Anteil tot angelieferter Tiere („Transporttote“, „tot angeliefert“, „dead on arrival (DOA)“, „Federtote“)." 
	branches = ["Legehenne"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "verletzt"
					short-description = "Durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere"
					long-description = "Durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere: z.B. Brüche von Schenkel und Flügel, Hämatome, teils auch unter Bezeichnung „Fangfehler“ oder „Fangschäden“"
					images = [ ]
				} 
				Var "B" {
					name = "tot"
					short-description = "Tot am Schlachthof angelieferte Tiere"
					long-description = "Tot am Schlachthof angelieferte Tiere: „Transporttote“, „tot angeliefert“, „dead on arrival (DOA)“, „Federtote“"
					images = [ ]
				} //End-Var labelClass1
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "A) Verletzte Tiere"
			description = "Anteil der durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere"
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 5
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier kam verletzt am Schlachhof an."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,1 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere kamen verletzt am Schlachthof an, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,5
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "B) Tote Tiere"
			description = "Anteil der tot am Schlachthof angelieferte Tiere"
			expression = "(B / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 2
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier kam tot am Schlachthof an."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,05
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere kamen tot am Schlachthof an, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,3
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Verwürfe und Ursachen"
	description = "Verworfene Tiere können entweder nicht schlachtfähig oder genussuntauglich beurteilt worden sein. Solche Tiere haben sich in der Regel bereits am Ende der Legeperiode in einem schlechten Ernährungs- und Gesundheitszustand befunden."
	animal-category = "Legehennen"
	video-ref = {"title1" = "link to video"}
	what-and-why = <<EOF Verworfene Tiere können entweder nicht schlachtfähig oder genussuntauglich beurteilt worden sein. Solche Tiere haben sich in der Regel bereits am Ende der Legeperiode in einem schlechten Ernährungs- und Gesundheitszustand befunden. Beispielsweise weist ein hoher Anteil abgemagerter Tiere auf unzureichende Ernährung oder Erkrankungen hin. Krankhafte Veränderungen können durch Infektionen hervorgerufen sein, große Verletzungen durch Kannibalismus. 
Sofern möglich ist es sinnvoll, Verwurfsursachen differenziert auszuwerten, um Problemfelder zu erkennen und im nächsten Durchgang Gegenmaßnahmen ergreifen zu können. Zum Beispiel sollte die Fütterung optimiert und die Gesundheitsvorsorge überprüft werden, wenn es einen hohen Anteil abgemagerter Tieren gibt. Die Daten werden vom Schlachthof entsprechend der behördlichen Vorgaben erhoben und dem Tierhalter (gegebenenfalls nach Anfrage) zur Verfügung gestellt. Die Schlachthöfe melden die Verwurfsursachen unterschiedlich differenziert oder teilweise gar nicht zurück.
EOF
	when-and-how-often = "Nach Ende des Durchgangs Schlachthofrückmeldungen auswerten."
	which-and-how-many = "Gesamtherde"
	how = "A) Auswertung der Schlachthofrückmeldung hinsichtlich des Anteils verworfener Tiere insgesamt. B) Falls Verwurfsursachen angegeben sind, entsprechend differenziert auswerten." 
	branches = ["Legehenne"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "verworfen"
					short-description = "verworfene Tiere"
					long-description = "verworfene Tiere"
					images = [ ]
				} 
				Var "B" {
					name = "spezifische Verwurfsursache"
					short-description = "Tiere mit spezifischer Verwurfsursache"
					long-description = "Tiere mit spezifischer Verwurfsursache"
					images = [ ]
				} //End-Var labelClass1
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Verworfene Tiere"
			description = "Anteil der verworfenen Tiere"
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 5
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier wurde verworfen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,5 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere wurden verworfen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 3
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Verworfene Tiere mit spezifischer Verwurfsursache"
			description = "Anteil der verworfenen Tiere mit spezifischer Verwurfsursache"
			expression = "(B / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 15.5
				visual-upperbound = 75.0
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier mit spezifischer Verwurfsursache wurde verworfen."
					lowerbound = 60 //Dezimalzahlen wären hier genauso möglich
					upperbound = 100 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere mit spezifischer Verwurfsursachen wurden verworfen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0
					upperbound = 30
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel


