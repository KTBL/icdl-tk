					long-description = "Keine Verletzungen"
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Leichte Verletzungen"
					long-description = "Maximal 2 kleine (< 1 cm) Ver-letzungen (frisch oder verkrustet) pro Region"
					images = [ ]
					thumbnail = ""
				} 
				Var "C" {
					name = "Boniturnote 2"
					short-description = "Schwere Verletzungen"
					long-description = "Ab 3 kleinen (< 1 cm) Verletzungen oder mindestens 1 großen (≥ 1 cm) Verletzung (frisch oder verkrustet) pro Region."
					images = [ ]
					thumbnail = ""	
				} //End-Var labelClass1
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Hautverletzungen"
			description = "Anteil der Tiere mit Hautverletzungen"
