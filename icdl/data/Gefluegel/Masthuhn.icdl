Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Lahmheit - gemerzte oder separierte Tiere"
	description = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit (Immobilität) eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates hinweisen."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit (Immobilität) eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates (z. B. Entzündungen oder Entwicklungsstörungen im Gelenksbereich) hinweisen. Auch eine sehr breite Brustmuskulatur zieht eine Beeinträchtigung der Gehfähigkeit nach sich. Schäden am Bewegungsapparat können mit Schmerzen verbunden sein und in jedem Fall die Bewegungsaktivität betroffener Tiere stark beeinträchtigen. Dies kann Probleme, wie mangelnde Futter- und Wasseraufnahme, nach sich ziehen. Bedingt durch vermehrtes Liegen können auch weitere Schäden am Tier, z. B. an der Brusthaut oder an den Fersenhöckern, auftreten. Das Risiko für das Auftreten von Lahmheiten kann unter anderem züchterisch beeinflusst werden, aber auch durch Faktoren der Haltungsumwelt, Pflege und Ernährung. Bei Einflüssen durch die Haltungsumwelt stehen die Einstreuqualität und mangelnde Bewegung aufgrund hoher Besatzdichten im Vordergrund. Die Luft- und Futterqualität sowie Futterzusammensetzung sind in diesem Zusammenhang ebenfalls zu berücksichtigen. Es wird dringend empfohlen sowohl die Anzahl der aufgrund Lahmheit gemerzten und separierten Tiere zu erfassen als auch das Ausmaß von Lahmheiten an einer Stichprobe von Tieren im Stall zu erheben."
	when-and-how-often = "Wöchentliches Auswerten der täglichen Aufzeichnungen."
	which-and-how-many = "Gesamtherde."
	how = "Entweder wöchentliche Summierung der täglichen Anzahl der wegen Lahmheit gemerzten oder separierten Tiere geteilt durch die Zahl eingestallter Tiere oder alternativ entsprechend der Berechnung der kumulativen Mortalitätsrate für die Ursache Lahmheit. Die Ergebnisse können sich geringfügig unterscheiden." 
	branches = ["Masthuhn1", "Masthuhn2"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Lahme Tiere"
					short-description = "Tiere, die wegen Lahmheit separiert oder gemerzt wurden"
					long-description = "Tiere, die wegen Lahmheit separiert oder gemerzt wurden"
					images = [ ]
				} 
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Lahmheit"
			description = "Anteil der Tiere, die wegen Lahmheit separiert oder gemerzt wurden"
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 2
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier wurde wegen Lahmheit separiert oder gemerzt."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,05 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere wurden wegen Lahmheit separiert oder gemerzt, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,25
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
