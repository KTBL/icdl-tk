Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Antibiotikaeinsatz"
	description = "Zur Behandlung von Infektionskrankheiten ist die Verabreichung von Antibiotika im Bedarfsfall aus Tierschutzsicht geboten. Ein hoher Antibiotikaeinsatz weist auf eine hohe Erkrankungsrate und damit möglicherweise auf Probleme in Haltung oder Management hin."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Zur Behandlung von Infektionskrankheiten ist die Verabreichung von Antibiotika im Bedarfsfall aus Tierschutzsicht geboten. Ein hoher Antibiotikaeinsatz weist auf eine hohe Erkrankungsrate und damit möglicherweise auf Probleme in Haltung oder Management hin. Relevant sind die Behandlungsdauer, die Anzahl der eingesetzten Wirkstoffe und die Anzahl der behandelten Tiere. Der betriebseigene Antibiotikaverbrauch kann z. B. mithilfe des Monitorings der Qualität und Sicherheit GmbH (QS) bewertet und eingeordnet werden. Die halbjährliche Auswertung der betriebsbezogenen Daten bietet Tierhaltern die Möglichkeit, den betriebseigenen Antibiotikaeinsatz mit dem anderer Betriebe zu vergleichen (Benchmarking). Zusätzlich empfiehlt es sich, den Antibiotikaverbrauch pro Durchgang (Therapieindex je Herde) auszuwerten, da der Bezug auf die Herde eine zielgenauere Information für die betriebliche Eigenkontrolle liefert. Der Antibiotikaeinsatz muss stets im Zusammenhang mit anderen Indikatoren, wie z. B. den Tierverlusten, bewertet werden. Die berechneten Therapieindizes können zum innerbetrieblichen Vergleich über mehrere Durchgänge genutzt werden."
	when-and-how-often = "Auswerten des Antibiotikaeinsatzes nach Ende jedes Durchgangs sowie z. B. anhand des halbjährlichen QS-Antibiotikamonitorings."
	which-and-how-many = "Gesamtherde."
	how = "Zur Vereinfachung der verschiedenen Dokumentationsaufgaben wird hier die gleiche Vorgehensweise wie im Rahmen des QS-Antibiotikamonitorings vorgeschlagen. Sie unterscheidet sich von der Formel nach § 58c Arzneimittelgesetz (AMG 2005) dadurch, dass der Arzneimitteleinsatz pro Herde berechnet wird und nicht für den durchschnittlichen Tierbestand im Kalenderhalbjahr. Der Therapieindex je Herde drückt aus, wie viele Behandlungseinheiten pro eingestalltem Tier in der jeweiligen Herde verabreicht wurden (siehe Formel A). Für das QS-Antibiotikamonitoring wird die Summe aller Therapieindizes je Herde für das letzte Halbjahr berechnet und durch die jeweilige Herdenanzahl in diesem Zeitraum geteilt (siehe Formel B). Auswertung der Dokumentation des Antibiotikaeinsatzes je Herde A) bzw. je Halbjahr B). " 
	branches = ["Masthuhn1", "Masthuhn2"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Antibiotika"
					short-description = "Behandlungseinheiten mit Antibiotika"
					long-description = "Anwendungsdauer inklusive Wirktage ∙ Anzahl Wirkstoffe ∙ Anzahl behandelter Tiere = Behandlungseinheit"
					images = [ ]
				} 
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Therapieindex je Herde"
			description = "Anteil der Tiere mit Behandlungseinheiten von Antibiotika"
			expression = "A / herd_size"
			Ranges {
				unit = "%|m²|kg|..."
				visual-lowerbound = 15.5
				visual-upperbound = 75.0
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat xy."
					lowerbound = 60 //Dezimalzahlen wären hier genauso möglich
					upperbound = 100 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben XY, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0
					upperbound = 30
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Therapieindex je Halbjahr"
			description = "Summe der Therapieindizes je Halbjahr durch die Herdenanzahl je Halbjahr"
			expression = ""
			Ranges {
				unit = "%|m²|kg|..."
				visual-lowerbound = 15.5
				visual-upperbound = 75.0
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat xy."
					lowerbound = 60 //Dezimalzahlen wären hier genauso möglich
					upperbound = 100 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben XY, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0
					upperbound = 30
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Verletzt oder tot angelieferte Tiere"
	description = "Die Zahl der verletzt oder tot angelieferten Tiere wird beeinflusst durch Erkrankungen und Vorschädigungen im Bestand, unsachgemäße Behandlung der Tiere beim Fangen und Verbringen in die Transportbehältnisse sowie beim Entladen, zu hohe oder zu niedrige Besatzdichten in den Behältnissen, Stress durch hohe Temperaturen und Luftfeuchte während des Verladens und Transports, die Fahrweise beim Transport und zu lange Wartezeiten am Schlachthof."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Verletzt oder tot am Schlachthof angelieferte Tiere haben in der Regel starke Beeinträchtigungen des Wohlergehens durch die Verletzungen oder bis zum Eintritt des Todes erfahren. Die Zahl der verletzt oder tot angelieferten Tiere wird beeinflusst durch Erkrankungen und Vorschädigungen im Bestand, unsachgemäße Behandlung der Tiere beim Fangen und Verbringen in die Transportbehältnisse sowie beim Entladen, zu hohe oder zu niedrige Besatzdichten in den Behältnissen, Stress durch hohe Temperaturen und Luftfeuchte während des Verladens und Transports (hier auch zu niedrige Temperaturen), die Fahrweise beim Transport und zu lange Wartezeiten am Schlachthof. Bei auffällig vielen tot angelieferten Tieren sollte die Höhe der Mortalität in der Mast mit betrachtet werden. Ein mögliches Krankheits-/Seuchengeschehen muss als potenzielle Ursache berücksichtigt werden. Frische Brüche von Schenkeln oder Flügeln sowie Hämatome, frische Verletzungen und Kratzer sind in der Regel durch Fangen, Verladen und/oder Transport verursacht, während ältere Verletzungen schon während der Haltungsperiode entstanden sind. Viele Schlachthöfe melden die durch Fangen, Verladen und Transport verletzten Tiere nicht separat zurück, daher kann dieser Teilindikator unter Umständen nicht ausgewertet werden."
	when-and-how-often = "Nach Ende des Durchgangs die vom Schlachthof - gegebenenfalls auf Nachfrage - erhaltenen Daten auswerten."
	which-and-how-many = "Gesamtherde."
	how = "Auswertung der Schlachthofrückmeldung bezüglich A) Anteil durch Fangen, Verladen und Transport verletzt angelieferter Tiere (z. B. Brüche von Schenkeln und Flügeln, Hämatome, teils auch unter Bezeichnung „Fangfehler“ oder „Fangschäden“ aufgelistet) und B) Anteil tot angelieferter Tiere („Transporttote“, „tot angeliefert“, „dead on arrival (DOA)“, „Federtote“)." 
	branches = ["Masthuhn2"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "verletzt"
					short-description = "Durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere"
					long-description = "Durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere: z.B. Brüche von Schenkeln und Flügeln, Hämatome, teils auch unter Bezeichnung „Fangfehler“ oder „Fangschäden“"
					images = [ ]
				} 
				Var "B" {
					name = "tot"
					short-description = "Tot am Schlachthof angelieferte Tiere"
					long-description = "Tot am Schlachthof angelieferte Tiere"
					images = [ ]
				} //End-Var labelClass1
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "A) Verletzte Tiere"
			description = "Anteil der durch Fangen, Verladen und Transport verletzte am Schlachthof angelieferte Tiere"
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 5
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier kam verletzt am Schlachhof an."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,1
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere kamen verletzt am Schlachthof an, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,5
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "B) Tote Tiere"
			description = "Anteil der tot am Schlachthof angelieferte Tiere"
			expression = "(B / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 5
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier kam tot am Schlachthof an."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,05 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere kamen tot am Schlachthof an, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,3
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Verwürfe und Ursachen"
	description = "„Verworfene ganze Tierkörper“ sowie gegebenenfalls „verworfene Teilstücke“ geben Hinweise auf Schäden am Tier und seinen Gesundheitszustand."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "„Verworfene ganze Tierkörper“ sowie gegebenenfalls „verworfene Teilstücke“ geben Hinweise auf Schäden am Tier und seinen Gesundheitszustand. Mögliche Ursachen für den Verwurf sind unter anderem unzureichende Ausblutung, Hämatome, Verletzungen (z. B. tiefe Dermatitis), Bauchwassersucht, Abmagerung und Allgemeinerkrankungen. Es ist sinnvoll, die Verwurfsursachen differenziert auszuwerten, um mögliche Problemfelder zu erkennen und im nächsten Durchgang Gegenmaßnahmen ergreifen zu können. Zum Beispiel können vermehrt abgemagerte Tiere auf Probleme mit der Futterversorgung hinweisen. Die Jahreszeit, das Mastendgewicht und die damit verbundene Mastdauer sowie die Besatzdichte können unter anderem einen Einfluss auf die Höhe der Verwürfe am Schlachthof haben. Die Daten werden vom Schlachthof entsprechend der behördlichen Vorgaben erhoben und dem Tierhalter (gegebenenfalls auf Anfrage) zur Verfügung gestellt. Die Schlachthöfe melden die Verwurfsursachen unterschiedlich differenziert oder teilweise gar nicht zurück."
	when-and-how-often = "Nach Ende des Durchgangs die vom Schlachthof - gegebenenfalls auf Nachfrage - erhaltenen Daten auswerten."
	which-and-how-many = "Gesamtherde."
	how = "Auswertung der Schlachthofrückmeldung hinsichtlich des Anteils verworfener Tiere insgesamt A). Falls Verwurfsursachen angegeben sind, entsprechend differenziert auswerten B)." 
	branches = ["Masthuhn2"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "ohne spezifische Ursache"
					short-description = "verworfene Tiere ohne spezifische Ursache"
					long-description = "verworfene Tiere ohne spezifische Ursache"
					images = [ ]
				} 
				Var "B" {
					name = "Tiefe Dermatitis"
					short-description = "Tiere mit spezifischer Verwurfsursache: Tiefe Dermatitis"
					long-description = "Tiere mit spezifischer Verwurfsursache: Tiefe Dermatitis"
					images = [ ]
				} //End-Var labelClass1
				Var "C" {
					name = "Fersenhockerveränderungen"
					short-description = "Tiere mit spezifischer Verwurfsursache: Fersenhockerveränderungen"
					long-description = "Tiere mit spezifischer Verwurfsursache: Fersenhockerveränderungen"
					images = [ ]
				} 
				Var "D" {
					name = "Fussballenveränderung"
					short-description = "Tiere mit spezifischer Verwurfsursache: Fussballenveränderung"
					long-description = "Tiere mit spezifischer Verwurfsursache: Fussballenveränderung"
					images = [ ]
				} //End-Var labelClass1
				Var "E" {
					name = "schwere Fussballenveränderung"
					short-description = "Tiere mit spezifischer Verwurfsursache: schwere Fussballenveränderung"
					long-description = "Tiere mit spezifischer Verwurfsursache: schwere Fussballenveränderung"
					images = [ ]
				} //End-Var labelClass1
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Verworfene Tiere"
			description = "Anteil der verworfenen Tiere"
			expression = "((A + B + C + D) / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 3
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier wurde verworfen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0,5 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere wurden verworfen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 1,5
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
