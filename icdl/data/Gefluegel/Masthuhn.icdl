					name = "Fersenhöckerveränderungen"
					short-description = "Tiere mit Fersenhöckerveränderungen"
					long-description = "Tiere mit Fersenhöckerveränderungen"
					images = [ ]
				}
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Fersenhöckerveränderungen "
			description = "Anteil der Tiere mit Fersenhöckerveränderungen "
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 15
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Fersenhöckerveränderungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 4 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Fersenhöckerveränderungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 12
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
