Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Hautverletzungen - Schlachthofrückmeldungen"
	description = "Hautverletzungen entstehen bei Masthühnern insbesondere durch die Krallen anderer Tiere und ist Tierschutzrechtlich und wirtschaftlich relevant."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Hautverletzungen entstehen bei Masthühnern insbesondere durch die Krallen anderer Tiere, z. B. wenn die Tiere bei Flucht oder Panik übereinander laufen. In Ausnahmefällen können Hautverletzungen auch durch gegenseitiges Bepicken, d. h. Kannibalismus, entstehen. Neben den Schmerzen durch die Verletzung können auch bei oberflächlichen Hautverletzungen Krankheitserreger tieferliegendes Gewebe infizieren, was zu großflächigen und schmerzhaften Entzündungsherden mit Eiterbildung im Unterhautgewebe führen kann (z. B. „tiefe Dermatitis“, „Unterhautvereiterung“). Bei den betroffenen Tieren wird am Schlachthof meist der ganze Tierkörper verworfen, sodass neben der Tierschutzrelevanz auch ein hoher ökonomischer Schaden entsteht. Gründe für vermehrte Hautverletzungen sind hohe Besatzdichten, hohe Stalltemperaturen, Schreckhaftigkeit sowie geringe Befiederung und eventuell ungünstige Lichtverhältnisse. Die mit Hautverletzung assoziierten Verwurfskategorien am Schlachthof, z. B. „tiefe Dermatitis“, erfassen jedoch nur die schweren Hautverletzungen, daher empfiehlt es sich, diesen Indikator zusätzlich im Stall zu erheben."
	when-and-how-often = "Nach Ende des Durchgangs die vom Schlachthof - gegebenenfalls auf Nachfrage - erhaltenen Daten in Bezug auf Verwurfskategorien auswerten, die mit Hautverletzung assoziiert sind, wie „tiefe Dermatitis“, „(Unter-)Hautentzündung“ oder „Unterhaut-Vereiterung“. Werden diese Verwurfskategorien nicht zurückgemeldet, sollte im Betrieb kurz vor Ausstallung der Herde eine Bonitierung bezüglich Hautverletzungen durchgeführt werden."
	which-and-how-many = "Gesamtherde."
	how = "Auswertung der Schlachthofrückmeldung hinsichtlich des Anteils Tiere mit Hautverletzungen (z. B. „tiefe Dermatitis“, „Unterhautentzündung“, „Unterhaut-Vereiterung“)." 
	branches = ["Masthuhn1", "Masthuhn2"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "Report"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Hautverletzungen"
					short-description = "Tiere mit Hautverletzungen"
					long-description = "Tiere mit Hautverletzungen (z. B. „tiefe Dermatitis“, „Unterhautentzündung“, „Unterhaut-Vereiterung“)"
					images = [ ]
				} 
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Hautverletzungen"
			description = "Anteil der Tiere mit Hautverletzungen"
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 50
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Hautverletzungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 8 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Hautverletzungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 30
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Hautverletzungen - Erfassung im Stall"
	description = "Hautverletzungen entstehen bei Masthühnern insbesondere durch die Krallen anderer Tiere und ist Tierschutzrechtlich und wirtschaftlich relevant."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Hautverletzungen entstehen bei Masthühnern insbesondere durch die Krallen anderer Tiere, z. B. wenn die Tiere bei Flucht oder Panik übereinander laufen. In Ausnahmefällen können Hautverletzungen auch durch gegenseitiges Bepicken, d. h. Kannibalismus, entstehen. Neben den Schmerzen durch die Verletzung können auch bei oberflächlichen Hautverletzungen Krankheitserreger tieferliegendes Gewebe infizieren, was zu großflächigen und schmerzhaften Entzündungsherden mit Eiterbildung im Unterhautgewebe führen kann (z. B. „tiefe Dermatitis“, „Unterhautvereiterung“). Bei den betroffenen Tieren wird am Schlachthof meist der ganze Tierkörper verworfen, sodass neben der Tierschutzrelevanz auch ein hoher ökonomischer Schaden entsteht. Gründe für vermehrte Hautverletzungen sind hohe Besatzdichten, hohe Stalltemperaturen, Schreckhaftigkeit sowie geringe Befiederung und eventuell ungünstige Lichtverhältnisse. Die mit Hautverletzung assoziierten Verwurfskategorien am Schlachthof, z. B. „tiefe Dermatitis“, erfassen jedoch nur die schweren Hautverletzungen, daher empfiehlt es sich, diesen Indikator zusätzlich im Stall zu erheben."
	when-and-how-often = "Anfang der letzten Lebenswoche vor der Schlachtung erheben und auswerten."
	which-and-how-many = "50 Tiere je Herde möglichst zufällig aus verschiedenen Stallbereichen (vorne, Mitte, hinten usw.) greifen."
	how = "Beurteilung hinsichtlich Hautverletzungen, d. h. Pick- und Katzverletzungen, an Flügeln, Rücken, Bauch sowie Oberschenkeln bei guter Beleuchtung unter Zurückstreichen des Gefieders. Dokumentiert wird die Boniturnote der am schlechtesten bewerteten Körperregion. Bei Grenzfällen zwischen den beiden Boniturnoten wird die bessere Note gegeben." 
	branches = ["Masthuhn2"]
	sorting = 10
	Assessment {
		sample = ""
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "Keine Hautverletzungen"
					long-description = "Keine Hautverletzungen"
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Leichte Hautverletzungen"
					long-description = "Maximal stecknadelkopfgroße (< 0,3 cm) Pickverletzungen (frisch oder verkrustet)"
					images = [ ]
					thumbnail = ""
				} //End-Var labelClass1
				Var "C" {
					name = "Boniturnote 2"
					short-description = "Schwere Hautverletzungen"
					long-description = "Mindestens 1 große Pickverletzung (≥ 0,3 cm) oder jegliche Kratzer (frisch oder verkrustet)"
					images = [ ]
					thumbnail = ""
				} //End-Var labelClass1
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Alle Hautverletzungen"
			description = "Anteil der Tiere, die Hautverletzungen haben."
			expression = "((B + C) / (A + B * C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 50
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Hautverletzungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 8 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Hautverletzungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 30
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Schwere Hautverletzungen"
			description = "Anteil der Tiere, die schwere Hautverletzungen haben."
			expression = "(C / (A + B * C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 10
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat schwere Hautverletzungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 2 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben schwere Hautverlerletzungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 6
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Fersenhöckerveränderungen - Schlachthofrückmeldung"
	description = "Hautschäden an den Fersenhöckern werden durch höhere Gewichte und fortschreitendes Alter der Tiere, bei Kontakt mit zu feuchter Einstreu und dem – u. a. aus Harnsäure entstehenden - Ammoniak begünstigt."
	animal-category = "Masthuhn"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Hautschäden an den Fersenhöckern werden durch höhere Gewichte und fortschreitendes Alter der Tiere, bei Kontakt mit zu feuchter Einstreu und dem – u. a. aus Harnsäure entstehenden - Ammoniak begünstigt. Insbesondere bei langen Liegezeiten können zunächst dunkle Verfärbungen und Entzündungen entstehen. In späteren Stadien können sich daraus abgestorbene (nekrotische) Bereiche entwickeln, die bis in tiefe Hautschichten reichen („Hock burns“). Die Fersenhöckerveränderungen werden von manchen Schlachthöfen nicht zurückgemeldet, daher empfiehlt es sich, diesen Indikator zusätzlich im Stall zu erheben."
	when-and-how-often = "Nach Ende des Durchgangs die vom Schlachthof - gegebenenfalls auf Nachfrage - erhaltenen Daten auswerten. Falls keine Fersenhöckerveränderungen erfasst werden, sollte im Betrieb vor Ausstallung der Herde eine Bonitierung im Stall durchgeführt werden."
	which-and-how-many = "Gesamtherde"
	how = "Auswertung der Schlachthofrückmeldungen hinsichtlich des Anteils Tiere mit Fersenhöckerveränderungen (auch als „Hock burns“, „Kontaktdermatitis Fersen“ aufgeführt)." 
	branches = ["Masthuhn2"]
	sorting = 10
