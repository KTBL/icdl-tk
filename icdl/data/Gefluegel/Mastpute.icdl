Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Außmaß und Qualität des Schnabelkürzens"
	description = "Das Kürzen der Schnabelspitze bei Puten ist grundsätzlich verboten. Abweichend von diesem Verbot kann die zuständige Behörde das Kürzen erlauben, wenn glaubhaft dargelegt wird, dass der Eingriff im Hinblick auf die vorgesehene Nutzung zum Schutz der Tiere unerlässlich ist."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Das Kürzen der Schnabelspitze bei Puten ist grundsätzlich verboten. Abweichend von diesem Verbot kann die zuständige Behörde das Kürzen erlauben, wenn glaubhaft dargelegt wird, dass der Eingriff im Hinblick auf die vorgesehene Nutzung zum Schutz der Tiere unerlässlich ist (§ 6 (3) Nr. 2 TierSchG). Zur Vermeidung schwerwiegender Schäden durch Federpicken und Kannibalismus ist dieser Eingriff unter konventionellen Haltungsbedingungen derzeit noch gängige Praxis. Das beim Eintagsküken in der Brüterei durchgeführte Kürzen der Schnabelspitze stellt einen schmerzhaften Eingriff dar. Trotz automatisierter Durchführung kann es vorkommen, dass die Schnäbel uneinheitlich oder zu stark gekürzt werden. Dadurch kann es später im Haltungsbetrieb unter anderem zu Einschränkungen bei der Futteraufnahme und Gefiederpflege kommen. Da sich das Ergebnis des Kürzens erst nach Abfall der Schnabelspitze (ca. 7. bis 10. Lebenstag) beurteilen lässt, sollte die Qualität des Eingriffs zu diesem Zeitpunkt routinemäßig kontrolliert werden. Bei unsachgemäßer Kürzung sollte eine entsprechende Rückmeldung an die Brüterei erfolgen. Zusätzlich sollte zum Ende der Aufzucht eine Bonitierung des Schnabelzustands durchgeführt werden, um gegebenenfalls den Anteil an Puten mit unzureichendem Schnabelschluss zu ermitteln, um daraus resultierende mögliche Verhaltenseinschränkungen oder Probleme bei der Futteraufnahme zu identifizieren."
	when-and-how-often = "Falls die Mastputen zugekauft werden: Bei schnabelgekürzten Tieren zum Einstallen erheben und auswerten."
	which-and-how-many = "50 Tiere je Herde möglichst zufällig aus verschiedenen Stallbereichen (vorne, Mitte, hinten usw.) greifen."
	how = "Beurteilung des Ausmaßes (Abstand Schnabelspitze zu Nasenloch) und der Qualität (Verhältnis von Ober- zu Unterschnabel) des Schnabelkürzens bei ausreichender Beleuchtung. Am Ende der Aufzucht sollte der Schnabelschluss weitgehend wiederhergestellt sein. Betrachtung des Schnabels von der Seite und vorne. Bei Grenzfällen zwischen zwei Boniturnoten wird die bessere Note gegeben." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "50"
		default_type = "By_Animal"        
		type_changeable = false 
		presentation = "classification"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "Ungekürzter Schnabel"
					long-description = "Von der Seite gesehen ragt der Oberschnabel bogenförmig über den Unterschnabel. Von vorn keine Öffnung sichtbar."
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Mäßig verkürzter Oberschnabel"
					long-description = "Von der Seite gesehen sind Ober- und Unterschnabel in etwa gleich lang. Von vorn ist nur eine kleine Öffnung sichtbar und der Unterschnabel ist nicht verkürzt."
					images = [ ]
					thumbnail = ""
				}
				Var "C" {
					name = "Boniturnote 2"
					short-description = "Stark verkürzter Oberschnabel"
					long-description = "Von der Seite gesehen ist Oberschnabel deutlich kürzer als Unterschnabel. Von vorn ist eine deutliche Öffnung sichtbar oder der Unter-schnabel ist verkürzt."
					images = [ ]
					thumbnail = ""
				} 
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Verürzter Oberschnabel"
			description = "Anteil der Tiere mit stark verkürztem Oberschnabel"
			expression = "(C / (A + B + C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 10
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat einen verkürzten Oberschnabel."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 2 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben verkürzte Oberschnäbel, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 6
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Gefiederschäden"
	description = "Ein intaktes Gefieder erfüllt eine Schutzfunktion gegenüber Umwelteinflüssen und spielt eine entscheidende Rolle bei der Temperaturregulation."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Ein intaktes Gefieder erfüllt eine Schutzfunktion gegenüber Umwelteinflüssen und spielt eine entscheidende Rolle bei der Temperaturregulation. Die Vollständigkeit des Gefieders gibt Hinweise auf die Unterbringung, Versorgung und Pflege der Tiere. Gefiederschäden und -verluste können durch Federpicken, aber auch durch Abrieb an Haltungseinrichtungen, am Boden oder anderen Tieren entstehen. Das Herausziehen von Federn durch Artgenossen ist schmerzhaft und kann zu Verletzungen und in der Folge zu Kannibalismus führen. Gefiederschäden weisen darauf hin, dass die Tiere mit den Haltungs- und Fütterungsbedingungen nicht zurechtkommen. Bei gehäuft auftretenden Gefiederschäden sollten Managementmaßnahmen zur Verhinderung von Federpicken und Kannibalismus durchgeführt werden. Von Federpicken vornehmlich betroffene Körperregionen bei Puten sind der Hals sowie Rücken mit Flügeln und Schwanzansatz."
	when-and-how-often = "Zum Einstallen sowie in der 12. und 16. Lebenswoche erheben und auswerten."
	which-and-how-many = "50 Tiere je Herde möglichst zufällig aus verschiedenen Stallbereichen (vorne, Mitte, hinten usw.) greifen."
	how = "Beurteilung der Vollständigkeit des Gefieders unter Zurückstreichen der Federn an Rücken mit Hals und Schwanzansatz sowie Flügeln. Dokumentiert wird dabei die Boniturnote der am schlechtesten bewerteten Körperregion. Bei Grenzfällen zwischen zwei Boniturnoten wird die bessere Note gegeben." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "50"
		default_type = "By_Animal"        
		type_changeable = false 
		presentation = "classification"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "Keine Gefiederschäden"
					long-description = "Maximal 2 fehlende Federn an derselben Stelle"
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Leichte Gefiederschäden"
					long-description = "Mindestens 1 kleine federlose Stelle (ab 3 fehlenden Federn) bis federlose Stelle < 5 cm längster Durchmesser"
					images = [ ]
					thumbnail = ""
				} 
				Var "C" {
					name = "Boniturnote 2"
					short-description = "Schwere Gefiederschäden"
					long-description = "Mindestens 1 großflächige federlose Stelle ≥ 5 cm längster Durchmesser"
					images = [ ]
					thumbnail = ""
				} //End-Var labelClass1
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Gefiederschäden"
			description = "Anteil der Tiere mit Gefiederschäden"
			expression = "((B + C) / (A + B + C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 10
				visual-upperbound = 50
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Gefiederschäden."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 6 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Gefiederschäden, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 30
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Schwere Gefiederschäden"
			description = "Anteil der Tiere mit schweren Gefiederschäden"
			expression = "(C / (A + B + C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 15
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat schwere Gefiederschäden."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 2 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben schwere Gefiederschäden, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 10
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Hautverletzungen - verendete, gemerzte oder separierte Tiere"
	description = "Hautverletzungen treten vorwiegend als Pick- oder Kratzverletzungen auf und stellen einen Schaden am Tier dar, der je nach Ausmaß und Tiefe mit mehr oder weniger starken Schmerzen einhergeht und Infektionen begünstigen kann."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Hautverletzungen treten vorwiegend als Pick- oder Kratzverletzungen auf und stellen einen Schaden am Tier dar, der je nach Ausmaß und Tiefe mit mehr oder weniger starken Schmerzen einhergeht und Infektionen begünstigen kann. Die Verletzungen sind häufig an den Flügeln, am Rücken und Schwanz sowie an gering bis nicht befiederten Hautpartien wie Kopf und Kopfanhängen, Hals und der Kloake zu sehen. Hautverletzungen an den Kopfanhängen entstehen häufig mit einsetzender Geschlechtsreife durch kämpferische Auseinandersetzungen, insbesondere bei den Hähnen. Beschädigendes Picken kann aber auch in Form von Kannibalismus auftreten, indem diese Verletzungen, oder z. B. häufig Kratzverletzungen am Rücken, nachfolgend von Artgenossen bepickt werden, was zu großflächigen Hautverletzungen führen kann. Kratzverletzungen treten oft im Zusammenhang mit gesteigerter Unruhe innerhalb einer Herde auf. Kratzverletzungen können auch bei Fluchtreaktionen, beim Treiben zum Verladen und beim Transport entstehen. Kannibalismus kann durch Stressoren, wie z. B. schlechtes Stallklima, zu hohe Besatzdichte oder Nährstoffmangel begünstigt werden. Eine weitere Rolle spielt die reizarme Umgebung in den meisten Ställen, wodurch die Tiere zu wenig beschäftigt sind. Insgesamt weisen gehäuft auftretende Hautverletzungen auf unzureichende Haltungs- und Managementbedingungen hin. Tiere mit frischen Verletzungen müssen unverzüglich separiert und erforderlichenfalls versorgt werden, weil sie das Risiko für weiteres verletzendes Picken in der Herde erhöhen und die Heilungschancen des betroffenen Tieres sonst gering sind. Bei schwerwiegenden Verletzungen ohne Aussicht auf Heilung sind die betreffenden Tiere zu merzen. Die Dokumentation der täglich aufgrund von Hautverletzungen verendeten, gemerzten und separierten Puten ermöglicht einen Überblick bezüglich des Auftretens von verletzendem Picken in der Herde. Um auch kleine Verletzungen erkennen zu können, sollte zusätzlich das Ausmaß von Hautverletzungen an einer Stichprobe von Tieren im Stall erhoben werden."
	when-and-how-often = "Wöchentliches Auswerten der täglichen Aufzeichnungen."
	which-and-how-many = "Gesamtherde"
	how = "Wöchentliche Summierung der täglichen Anzahl der wegen Hautverletzung verendeten, gemerzten oder separierten Tiere geteilt durch die Zahl eingestallter Tiere." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Hautverletzung"
					short-description = "Wegen Hautverletzung verendeten, gemerzten oder separiertes Tier"
					long-description = "Wegen Hautverletzung verendeten, gemerzten oder separiertes Tier"
					images = [ ]
				} 
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Hautverletzung"
			description = "Anteil der wegen Hautverletzung verendeten, gemerzten oder separierten Tiere"
			expression = "(A / herd_size) * 100"	
				}
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

