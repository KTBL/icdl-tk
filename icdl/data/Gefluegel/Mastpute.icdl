Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Fussballenveränderungen - Erfassung im Stall"
	description = "Hauptrisikofaktor für die Entstehung von Fußballenveränderungen ist eine feuchte Einstreu. Ammoniak wird hier vermehrt freigesetzt und führt aufgrund seiner ätzenden Wirkung an der aufgeweichten Sohlenhaut zu mehr oder weniger starken Veränderungen."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Hauptrisikofaktor für die Entstehung von Fußballenveränderungen ist eine feuchte Einstreu. Ammoniak wird hier vermehrt freigesetzt und führt aufgrund seiner ätzenden Wirkung an der aufgeweichten Sohlenhaut zu mehr oder weniger starken Veränderungen. Eine anhaltende Beanspruchung und Reizung der Fußballenhaut kann zunächst zu einer starken Verhornung der Fuß- und Zehenballen führen. Zusätzlich können oberflächliche und schließlich tiefe Schäden bzw. Läsionen auftreten, die durch dunkle Farbveränderungen gekennzeichnet sind. Bakterien können derartige Hautschäden als Eintrittspforte nutzen und Entzündungen bis zu Geschwüren mit deutlichen Schwellungen hervorrufen. Aufgrund der Schmerzhaftigkeit, besonders im fortgeschrittenen Stadium, bewegen sich die betroffenen Tiere weniger; sie sitzen oder liegen vermehrt auf der Brust. Die Einstreufeuchte wird unter anderem durch die Einstreuart, Besatzdichte, Fütterung und Stallklimaführung beeinflusst. Auf Seiten der Tiere werden zudem Genetik, Geschlecht und Alter beeinflussende Effekte zugesprochen. Die Fußballenveränderungen werden von manchen Schlachthöfen nicht oder nur als gewichteter Fußballen-Score zurückgemeldet, daher sollte dieser Indikator vor der Ausstallung im Stall erhoben werden. Die Auswertungen können zum innerbetrieblichen Vergleich über mehrere Durchgänge herangezogen werden. Zusätzlich sollte der Fußballen-Score aus dem QS-Benchmark betrachtet werden, um sich mit anderen Betrieben vergleichen zu können."
	when-and-how-often = "Zum Einstallen sowie in der 12. und 16. Lebenswoche erheben und auswerten."
	which-and-how-many = "50 Tiere je Herde möglichst zufällig aus verschiedenen Stallbereichen (vorne, Mitte, hinten usw.) greifen."
	how = "Beurteilung von beiden Füßen nach Entfernung von anhaftendem Kot sowie Einstreu (z. B. mit Fingernagel oder Bürste) und bei guter Beleuchtung hinsichtlich Veränderung der Sohlen- und Zehenballen. Dokumentiert wird die Boniturnote des am schlechtesten bewerteten Fußes. Bei Grenzfällen zwischen zwei Boniturnoten wird die bessere Note gegeben. Die Boniturnoten (0, 1, 2) entsprechen dabei den Bezeichnungen des Gesundheitskontrollprogramms (A, B, C)." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "50"
		default_type = "By_Animal"        
		type_changeable = false 
		presentation = "classification"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "Keine Fußballenveränderungen"
					long-description = "Maximal stecknadelkopfgroße (< 0,3 cm), dunkle Verfärbung oder Zerstörung der oberen Hautschicht (Läsionen) am Sohlen- bzw. Zehenballen."
					images = [ ]
					thumbnail = ""
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "Leichte Fußballenveränderungen"
					long-description = "Dunkle Areale oder Läsionen, die bis zur Hälfte (50 %) des Sohlenballens betreffen oder bei unverändertem Sohlenballen dunkle Areale oder Läsionen bis zur Hälfte (50 %) der Fläche(n) eines oder mehrerer Zehenballen."
					images = [ ] 
					thumbnail = ""
				} 
				Var "C" {
					name = "Boniturnote 2"
					short-description = "Schwere Fußballenveränderungen"
					long-description = "Dunkle Areale oder Läsionen, die mindestens die Hälfte (50 %) des Sohlenballens betreffen oder bei unverändertem Sohlenballen dunkle Areale oder Läsionen, die mindestens die Hälfte (50 %) der Fläche(n) eines oder mehrerer Zehenballen betreffen oder Fußballen aufgrund starker, anhaftender Verschmutzung nicht beurteilbar."
					images = [ ]
					thumbnail = ""
				} 
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Fussballenveränderungen"
			description = "Anteil der Tiere mit Fussballenveränderungen"
			expression = "((B + C) / (A + B + C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 20
				visual-upperbound = 80
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Fussballenveränderungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 30 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Fussballenveränderungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 60
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Schwere Fussballenveränderungen"
			description = "Anteil der Tiere mit schweren Fussballenveränderungen"
			expression = "(C / (A + B + C)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 10
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat schwere Fussballenveränderungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 2 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben schwere Fussballenveränderungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 6
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Lahmheit - gemerzte uns separierte Tiere"
	description = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates hinweisen."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit (Immobilität) eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates (z. B. Entzündungen oder Entwicklungsstörungen im Gelenksbereich) hinweisen. Auch eine sehr breite Brustmuskulatur zieht eine Beeinträchtigung der Gehfähigkeit nach sich. Schäden am Bewegungsapparat können mit Schmerzen verbunden sein und in jedem Fall die Bewegungsaktivität betroffener Tiere stark beeinträchtigen. Dies kann Probleme, wie mangelnde Futter- und Wasseraufnahme, nach sich ziehen. Bedingt durch vermehrtes Liegen können auch weitere Schäden am Tier, z. B. an der Brusthaut oder an den Fersenhöckern, auftreten. Das Risiko für das Auftreten von Lahmheiten kann unter anderem züchterisch beeinflusst werden, aber auch durch Faktoren der Haltungsumwelt, Pflege und Ernährung. Bei Einflüssen durch die Haltungsumwelt stehen die Einstreuqualität und mangelnde Bewegung aufgrund hoher Besatzdichten im Vordergrund. Die Luft- und Futterqualität sowie Futterzusammensetzung sind in diesem Zusammenhang ebenfalls zu berücksichtigen. Es wird dringend empfohlen, sowohl die Anzahl der aufgrund Lahmheit gemerzten und separierten Tiere zu erfassen als auch das Ausmaß von Lahmheiten an einer Stichprobe von Tieren im Stall zu erheben."
	when-and-how-often = "Wöchentliches Auswerten der täglichen Aufzeichnungen"
	which-and-how-many = "Gesamtherde"
	how = "Wöchentliche Summierung der täglichen Anzahl der wegen Lahmheit gemerzten oder separierten Tiere geteilt durch die Zahl eingestallter Tiere." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "lahm"
					short-description = "täglichen Anzahl der wegen Lahmheit gemerzten oder separierten Tiere "
					long-description = "täglichen Anzahl der wegen Lahmheit gemerzten oder separierten Tiere "
					images = [ ]
				} 
			}
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Lahmheit"
			description = "Wöchentliche Rate der aufgrund von Lahmheit gemerzten oder separierten Tiere"
			expression = "(A / herd_size) * 100"	
				}
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Lahmheit - Erfassung im Stall"
	description = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates hinweisen."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Eine Lahmheit bis hin zur vollständigen Unbeweglichkeit (Immobilität) eines Tieres kann unter anderem auf Verletzungen oder verschiedene Erkrankungen des Bewegungsapparates (z. B. Entzündungen oder Entwicklungsstörungen im Gelenksbereich) hinweisen. Auch eine sehr breite Brustmuskulatur zieht eine Beeinträchtigung der Gehfähigkeit nach sich. Schäden am Bewegungsapparat können mit Schmerzen verbunden sein und in jedem Fall die Bewegungsaktivität betroffener Tiere stark beeinträchtigen. Dies kann Probleme, wie mangelnde Futter- und Wasseraufnahme, nach sich ziehen. Bedingt durch vermehrtes Liegen können auch weitere Schäden am Tier, z. B. an der Brusthaut oder an den Fersenhöckern, auftreten. Das Risiko für das Auftreten von Lahmheiten kann unter anderem züchterisch beeinflusst werden, aber auch durch Faktoren der Haltungsumwelt, Pflege und Ernährung. Bei Einflüssen durch die Haltungsumwelt stehen die Einstreuqualität und mangelnde Bewegung aufgrund hoher Besatzdichten im Vordergrund. Die Luft- und Futterqualität sowie Futterzusammensetzung sind in diesem Zusammenhang ebenfalls zu berücksichtigen. Es wird dringend empfohlen, sowohl die Anzahl der aufgrund Lahmheit gemerzten und separierten Tiere zu erfassen als auch das Ausmaß von Lahmheiten an einer Stichprobe von Tieren im Stall zu erheben."
	when-and-how-often = "Zum Einstallen sowie in der 12. und 16. Lebenswoche erheben und auswerten."
	which-and-how-many = "50 Tiere je Herde aus verschiedenen Stallbereichen (vorne, Mitte, hinten usw.) möglichst zufällig greifen."
	how = "Die Gehfähigkeit kann sowohl in Kombination mit den anderen Stichprobenindikatoren (Gefiederschäden, Hautverletzungen, Fußballenveränderungen) als auch im Rahmen einer gesonderten Lahmheitsbeurteilung nach der Transect-Methode erhoben werden. Bei der kombinierten Erhebung wird die Lahmheit vor dem Fangen der Tiere beurteilt, um eine Beeinflussung durch das Handling der Puten zu verhindern. Bei der Transect-Methode wird der Stall viermal entlang der Futtereinrichtungen durchschritten und die Gehfähigkeit von 50 zufällig ausgewählten Tieren beurteilt. Sitzende Puten werden durch vorsichtiges Auftreiben zum Laufen animiert, wobei die Tiere nicht aufgeschreckt werden sollten. Die Gehfähigkeit jeden Tieres wird einige Sekunden beobachtet. Die Beurteilung muss bei ausreichendem Licht durchgeführt werden. Bei Grenzfällen zwischen den beiden Boniturnoten wird die bessere Note gegeben." 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "50"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "Boniturnote 0"
					short-description = "normal"
					long-description = "Tier läuft normal oder nur geringgradig beeinträchtigt (maximal kleiner Defekt, z. B. übertrieben große Schritte oder unrunder Gang)"
					images = [ ]
				} 
				Var "B" {
					name = "Boniturnote 1"
					short-description = "deutlich lahm"
					long-description = <<EOF Mindestens eines der folgenden Kriterien trifft zu:
starkes Hinken
ruckartiges Fallen auf ein Bein
Bein ist beim Laufen heftig abgespreizt
mehrmalig setzt sich das Tier nach wenigen Schritten hin
Manövrierfähigkeit ist stark beeinträchtigt
Tier ist außerstande zu laufen, es bleibt sitzen (immobil)
EOF
					images = [ ]
				} 
			} 
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Lahmheit"
			description = "Anteil der Tiere, die deutlich Lahm sind"
			expression = "(B / (A + B)) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 8
				Targetrange {
					tag = "Zielbereich" 
					description = "Kein Tier ist deutlich lahm."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere sind deutlich lahm, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 4
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	
	} // End-Evaluation	
} // End-Indicator indicatorLabel

Indicator "indicatorLabel" {
	last-modified = "timestamp der letzten Änderung"
	name = "Tierverluste"
	description = "Tierverluste setzen sich aus im Stall verendeten und gemerzten Tieren zusammen, sind ein wichtiger Tierschutzindikator und ökonomisch von großer Bedeutung."
	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Tierverluste setzen sich aus im Stall verendeten und gemerzten Tieren zusammen. Sie sollten mit vermuteter Todesursache bzw. Merzungsgrund bei den täglichen Routinekontrollen in den vorhandenen Stallkarten notiert werden. Da den Tierverlusten in der Regel starke Beeinträchtigungen des Wohlergehens vorausgehen, ist die Höhe der Tierverluste ein wichtiger Tierschutzindikator. Neben ihrer Tierschutzrelevanz sind sie auch ökonomisch von großer Bedeutung. Ausgewertet werden sollten nicht nur die Höhe der Verluste, sondern auch deren vermutete Ursachen. So lassen sich gegebenenfalls Problemfelder im Betrieb erkennen und Verbesserungsmaßnahmen einleiten. Zusätzlich sollte die Mortalitätsrate je Durchgang (Aufzucht- bzw. Mastphase) zum innerbetrieblichen Vergleich über mehrere Durchgänge herangezogen werden."
	when-and-how-often = "Wöchentliches Auswerten der täglichen Aufzeichnungen und am Ende des Durchgangs"
	which-and-how-many = "Gesamtherde"
	how = "Berechnung von A) der kumulativen Mortalitätsrate je Lebenswoche, B) nach Ursachen getrennt (z. B. abgemagerte, erkrankte Tiere, Kannibalismus, Unfälle, gegebenenfalls Beutegreiferverluste und Sonstiges (inklusive Ursache nicht erkennbar)) sowie C) der kumulativen Mortalitätsrate" 
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "tote Tiere mit Ursache: abgemagert"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: abgemagert"
					images = [ ]
				}
				Var "B" {
					name = "tote Tiere mit Ursache: erkrankt"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: erkrankt"
					images = [ ]
				}
				Var "C" {
					name = "tote Tiere mit Ursache: lahm"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: lahme Tiere"
					images = [ ]
				}
				Var "D" {
					name = "tote Tiere mit Ursache: Kannibalismus"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: Kannibalismus bzw. ausgefressene Tiere"
					images = [ ]
				}
				Var "E" {
					name = "tote Tiere mit Ursache: Unfall"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: Unfälle"
					images = [ ]
				}
				Var "F" {
					name = "tote Tiere mit Ursache: Beutegreiferverluste"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: Beutegreiferverluste"
					images = [ ]
				}
				Var "G" {
					name = "tote Tiere mit Ursache: Hautverletzungen"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: Hautverletzungen"
					images = [ ]	
				}
				Var "H" {
					name = "tote Tiere mit Ursache: sonstiges"
					short-description = "Aufgrund spezifischer Ursache verendete und gemerzte Tiere"
					long-description = "Spezifische Ursache: Sonstiges (inklusive Ursache nicht erkennbar) "
					images = [ ]	
				}
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "A) Wöchentliche Mortalitätsrate"
			description = "Anteil der verendeten und gemerzten Tiere"
			expression = "((A + B + C + D + E + F + G + H) / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 15.5
				visual-upperbound = 75.0
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier ist verendet oder gemerzt worden."
					lowerbound = 60 //Dezimalzahlen wären hier genauso möglich
					upperbound = 100 
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere sind verendet oder gemerzt worden, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0
					upperbound = 30
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "B) Wöchentlich Mortalitätsrate aufgrund von Abmagerung"
			description = "Anteil der verendeten und gemerzten Tiere aufgrund Abmagerung."
			expression = "(A / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 15.5
				visual-upperbound = 75.0
