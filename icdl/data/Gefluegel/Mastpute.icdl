	animal-category = "Mastpute"
	video-ref = {"title1" = "link to video"}
	what-and-why = "Brusthautveränderungen inklusive Brustblasen stellen einen Schaden am Tier dar. Bei der Entstehung der Brusthautveränderung können sich anfänglich erste Druckstellen ausbilden, die sich verfestigen und entzünden. Eine Ursache ist das Ruhen der Tiere über einen längeren Zeitraum in Brustlage. Hierbei wirken sich eine harte und verschmutzte sowie nasse Einstreu und der dort entstehende Ammoniak negativ aus. Zudem können Erkrankungen des Bewegungsapparates die Liegezeiten erhöhen. Zu verminderter Bewegung kann aber auch eine höhere Umgebungstemperatur, wie sie insbesondere in den Sommermonaten auftritt, führen. Bei Hähnen kann die mögliche langsamere Befiederung Veränderungen im Brustbereich befördern. Auch wird eine erhöhte Gefährdung bei schnellwüchsigen Tieren und schweren Genetiken bzw. hohen Körpergewichten angenommen. Die Veränderungen an der Brusthaut umfassen Brustblasen, Brustknöpfe und Druckstellen. Hähne sind in der Regel häufiger und schwerer betroffen als Hennen. Neben der Tierschutzrelevanz ergibt sich auch ein wirtschaftlicher Schaden durch die Minderbewertung des Schlachttierkörpers. Einige Schlachthöfe melden die Brusthautveränderungen nicht separat zurück, daher kann dieser Indikator unter Umständen nicht ausgewertet werden."
	when-and-how-often = "Nach Ende des Durchgangs die vom Schlachthof – gegebenenfalls auf Nachfrage – erhaltenen Daten auswerten."
	which-and-how-many = "Gesamtherde"
	how = <<EOF Auswertung der Schlachthofrückmeldungen hinsichtlich des Anteils Tiere mit
1) Brusthautveränderungen insgesamt (Kategorie „B“, Umfangsvermehrungen bis einschließlich 5 cm im Durchmesser, teils auch als „Knöpfe“ bezeichnet und Kategorie „C“, Umfangsvermehrungen über 5 cm im Durchmesser) sowie
2) des Anteils Tiere nur mit hochgradigen Brusthautveränderungen (Kategorie „C“, Umfangsvermehrungen über 5 cm im Durchmesser).
EOF
	branches = ["Mastpute"]
	sorting = 10
	Assessment {
		sample = "herd_size"
		default_type = "By_Indicator"        
		type_changeable = false 
		presentation = "counter"        
		presentation_changeable = false 
		Options { // Options is only used if the type is "classification"
			Group {
				name = "Bonituren Gruppe 1"
				Var "A" {
					name = "B"
					short-description = "Brusthautveränderung Kategorie B"
					long-description = "Umfangsvermehrungen bis einschließlich 5 cm im Durchmesser, teils auch als „Knöpfe“ bezeichnet"
					images = [ ]
				} 
				Var "B" {
					name = "C"
					short-description = "Brusthautveränderung Kategorie C"
					long-description = "Umfangsvermehrungen über 5 cm im Durchmesser"
					images = [ ]
				} //End-Var labelClass1
			} // End-Group Bonituren Gruppe 1
		} //End-Options
	} //End-Assessment
	Evaluation {
		Formula {
			name = "Brusthautveränderungen insgesamt"
			description = "Anteil der Tiere mit Brusthautveränderungen der Kategorie B und C"
			expression = "((A + B) / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 6
				Targetrange {
					tag = "Zielbereich" 
					description = "Kaum ein Tier hat Brusthautveränderungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 1
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben Brusthautveränderungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 3
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
		Formula {
			name = "Hochgradige Brusthautveränderungen"
			description = "Anteil der Tiere mit Brusthautveränderungen der C"
			expression = "(B / herd_size) * 100"
			Ranges {
				unit = "%"
				visual-lowerbound = 0
				visual-upperbound = 2
				Targetrange {
					tag = "Zielbereich" 
					description = "Kein Tier hat hochgradige Brusthautveränderungen."
					lowerbound = 0 //Dezimalzahlen wären hier genauso möglich
					upperbound = 0
				}
				Alertrange {
					tag = "Alarmbereich"
					description = "Zu viele Tiere haben hochgradige Brusthautveränderungen, sofortiger Handlungsbedarf ist nötig!"
					lowerbound = 0,5
					upperbound = 100
				}
				Warningrange {
					tag = "Frühwarnbereich"
					description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"	
				}
			} // End-Ranges
		} // End-Formula
	} // End-Evaluation	
} // End-Indicator indicatorLabel

