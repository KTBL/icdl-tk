Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Abferkelquote"
  description = "Die Abferkelquote ist ein Maß für den Belegungserfolg und gibt an, wie viele belegte Sauen am Ende der Trächtigkeit abgeferkelt haben."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Die Abferkelquote (Abferkelrate) ist ein Maß für den Belegungserfolg und gibt an, wie viele belegte Sauen am Ende der Trächtigkeit abgeferkelt haben. Sie beträgt 100 %, wenn aus jeder Belegung ein Wurf hervorgegangen ist. Sie kann jedoch aufgrund von Umrauschen, Abgängen nach dem Belegen und Aborten geringer ausfallen. Dabei werden mit der Abferkelquote sowohl Spätaborte als auch leere Sauen berücksichtigt. Eine niedrige Abferkelquote kann ein Hinweis auf gesundheitliche Probleme oder Stress (Hitzestress, soziale Rangkämpfe) mit negativen Auswirkungen auf die Fruchtbarkeitsleistung im Bestand sein. Zusätzlich kann eine verringerte Abferkelquote auf Defizite im eigenen Besamungsmanagement hinweisen."
  when-and-how-often = "Fortlaufend erheben, halbjährlich auswerten"
  which-and-how-many = "Alle Sauen"
  how = "Die Abferkelquote wird vom Sauenplaner standardmäßig berechnet. Für die Auswertung innerhalb eines Erhebungshalbjahres können Sie sich die Abferkelquote nach Echtzeit (Berichtszeitraum „von – bis“) auslesen lassen."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "Report"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Wurf"
          short-description = "Wurf"
          long-description = "Wurf"
          images = [ ]
        }
        Var "B" {
          name = "Belegung"
          short-description = "Belegung"
          long-description = "Belegung"
          images = [ ]
        }
        //End-Var labelClass1
      }
      // End-Group Bonituren Gruppe 1
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Abferkelquote"
      description = "Anteil der Sauen, die am Ende der Trächtigkeit abgeferkelt haben, an der Gesamtzahl belegter Sauen."
      expression = (A / B) * 100
      Ranges {
        unit = "%"
        visual-lowerbound = 70
        visual-upperbound = 90
        Targetrange {
          tag = "Zielbereich"
          description = "Viele Sauen haben am Ende der Trächtigkeit abgeferkelt."
          lowerbound = 85
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 100
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu wenige Sauen haben am Ende der Trächtigkeit abgeferkelt, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 0
          upperbound = 75
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula

  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Wurfnummer bei Abgang"
  description = "Die durchschnittliche Wurfzahl aller aus der Herde abgehenden Sauen wird als Maßstab für Alter und Lebensleistung der Sauen herangezogen."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Die durchschnittliche Wurfnummer (Wurfzahl) aller aus der Herde abgehenden Sauen wird als Maßstab für Alter und Lebensleistung der Sauen herangezogen. Eine geringe Anzahl an Würfen je abgehender Sau kann entweder durch eine hohe Remontierungsrate, zu frühe Abgänge oder eine lan- ge Zwischenwurfzeit (z. B. durch erhöhte Umrauscherquote oder Abortrate) bedingt sein. Damit kann eine geringe durchschnittliche Wurfnummer auf verschiedene Gesundheitsprobleme im Bestand hinweisen."
  when-and-how-often = "Fortlaufend erheben, halbjährlich auswerten"
  which-and-how-many = "Alle Sauen"
  how = "Die durchschnittliche Wurfnummer bei Abgang wird von den meisten Sauenplanern standardmäßig berechnet. Für die Auswertung innerhalb eines Erhebungshalbjahres können Sie sich die mittlere Wurfnummer bei Abgang nach Echtzeit (Berichtszeitraum „von – bis“) auslesen lassen."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "Report"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Wurfnummer"
          short-description = "Wurfnummer der abgehenden Sau"
          long-description = "Wurfnummer der abgehenden Sau"
          images = [ ]
        }
        Var "B" {
          name = "abgehenden Sauen"
          short-description = "abgehenden Sauen"
          long-description = "abgehenden Sauen"
          images = [ ]
        }
        //End-Var labelClass1
      }
      // End-Group Bonituren Gruppe 1
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Wurfnummer"
      description = "Durchschnittliche Wurfnummer bei Abgang"
      expression = A / B
      Ranges {
        unit = ""
        visual-lowerbound = 1
        visual-upperbound = 7
        Targetrange {
          tag = "Zielbereich"
          description = "Die Wurfnummer war groß genug"
          lowerbound = 0
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 3.5
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere hatten eine zu geringe Wurfnummer, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 3.5
          upperbound = 2
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula
  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Tierverluste"
  description = "Tierverluste setzen sich aus verendeten und euthanasierten Tieren zusammen und sind Tierschutzrechtlich und wirtschaftlich interessant."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Tierverluste setzen sich aus verendeten und euthanasierten Tieren zusammen. Sie sind vom betriebsindividuellen Management sowie gegebenenfalls auch von technischen Mängeln der Haltungsbedingungen abhängig und haben – neben ihrer Tierschutzrelevanz – auch aus ökonomischer Sicht eine große Bedeutung. Zu den Ursachen zählen nicht infektiöse Gründe (z. B. Verletzungen, Herz-Kreislauf-Erkrankungen) sowie infektiöse Ursachen (z. B. PRRS). Da den Tierverlusten in der Regel starke Beeinträchtigungen des Wohlergehens vorausgehen, ist deren Höhe ein wichtiger Tierschutzindikator."
  when-and-how-often = "Fortlaufend erheben, halbjährlich auswerten"
  which-and-how-many = "Alle Sauen"
  how = "Die Tierverluste (verendete und euthanasierte Tiere) werden von Sauenplanern standardmäßig berechnet. Um bei Problemen die Planung von Gegenmaßnahmen zu vereinfachen, sollte bei der Datenerfassung im Sauenplaner stets auch die (vermutete) Ursache miterfasst werden. Für die Auswertung innerhalb eines Erhebungshalbjahres können Sie sich die Tierverluste nach Echtzeit (Berichtszeitraum „von … bis“) im Sauenplaner auslesen lassen."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "By_Indicator"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "verendet und euthanasiert"
          short-description = "Anzahl der verendeten und euthanasierten Tiere"
          long-description = "Anzahl der verendeten und euthanasierten Tiere"
          images = [ ]
        }
        Var "B" {
          name = "Anfangsbestand"
          short-description = "Anzahl Tiere am Anfang"
          long-description = "Anzahl Tiere am Anfang"
          images = [ ]
        }
        Var "C" {
          name = "Zugänge"
          short-description = "Tiere, die dazugekommen sind"
          long-description = "Tiere, die dazugekommen sind"
          images = [ ]
        }
        Var "D" {
          name = "Abgänge"
          short-description = "Tiere, die abgegangen sind"
          long-description = "Tiere, die abgegangen sind, zum Beispiel wegen Verkauf oder Schlachtung"
          images = [ ]
        }
      }
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Tierverluste"
      description = "Anteil der verendenten und euthanasierten Tiere am Gesamtbestand"
      expression = (A / (B + C - D)) * 100
      Ranges {
        unit = "%"
        visual-lowerbound = 0
        visual-upperbound = 10
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier ist verendet oder euthanasiert worden."
          lowerbound = 0
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 2
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere sind verendet oder euthanasiert worden, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 5
          upperbound = 100
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula
  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Antibiotikaeinsatz"
  description = "Ein hoher Antibiotikaeinsatz weist auf eine hohe Erkrankungsrate und damit möglicherweise auf Probleme in Haltung oder Management hin. "
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Die Verabreichung von Antibiotika zur Behandlung von bakteriellen Infektionen ist im Bedarfsfall aus Tierschutzsicht geboten. Ein hoher Antibiotikaeinsatz weist auf eine hohe Erkrankungsrate und damit möglicherweise auf Probleme in Haltung oder Management hin. Relevant sind die Behandlungsdauer, die Anzahl der eingesetzten Wirkstoffe und die Anzahl der behandelten Tiere. Risikofaktoren für bakterielle Infektionen sind z. B. Defizite in der Hygiene oder dem Stallklima, aber auch eine chronische Stressbelastung der Tiere, die eine erhöhte Infektionsanfälligkeit zur Folge haben kann."
  when-and-how-often = "Fortlaufend erheben; halbjährlich prüfen"
  which-and-how-many = "Alle Sauen"
  how = <<EOF
Der Antibiotikaeinsatz kann über 1) den Therapieindex bzw. alternativ 2) die Therapiehäufigkeit ermittelt werden:
1)Für am QS-System (Qualität und Sicherheit GmbH) teilnehmende Tierhalter nimmt der Tierarzt die Meldung des Antibiotikaeinsatzes vor. Tierhalter werden daraufhin halbjährlich über ihren Therapieindex und die Einstufung im Vergleich zu allen teilnehmenden Betrieben sowie über die betriebsinterne zeitliche Entwicklung informiert.
2)Alternativ kann die betriebliche halbjährliche Therapiehäufigkeit gemäß § 58c Arzneimittelgesetz (AMG 2005) ermittelt werden, indem der Antibiotikaeinsatz vom Schweinehalter oder Tierarzt in die Datenbank des HIT (Herkunftssicherungs- und Informationssystem für Tiere) eingetragen wird.
EOF
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "Report"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Antibiotikanwendung"
          short-description = "Anwendungsdauer inkl. Wirktage · Anzahl Wirkstoffe · Anzahl behandelter Tiere"
          long-description = "Anwendungsdauer inkl. Wirktage · Anzahl Wirkstoffe · Anzahl behandelter Tiere"
          images = [ ]
        }
      }
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Therapieindex"
      description = "Beschreibung der Formel"
    }
    // End-Formula
    Formula {
      name = "Therapiehäufigkeit"
      description = "Beschreibung der Formel"
      Ranges {
        unit = "%|m²|kg|..."
        visual-lowerbound = 15.5
        visual-upperbound = 75.0
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier hat xy."
          lowerbound = 60
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 100
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere haben XY, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 0
          upperbound = 30
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula
  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Schlachtbefunde"
  description = "Eine Allgemeine Beschreibung zusätzlich zu der detaillierten unten"
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Die Daten, die vom Schlachthof entsprechend der rechtlichen Vorgaben oder zusätzlich freiwillig erhoben werden und dem Tierhalter zur Verfügung gestellt werden, liefern Informatio- nen über Krankheitsgeschehen oder haltungsbedingte Schäden (Technopathien). Eine Auskunft über Schlachtbefunde von Sauen erfolgt gegebenenfalls erst nach Anfrage beim zuständigen Schlachthof. Folgende Daten werden von Schlachthöfen erhoben: Brustfellentzündungen (Pleuritiden), Bauchfellentzündungen (Peritonitiden), Lungenentzündungen (Pneumonien), Gelenkentzündungen (Arthritiden), Leberveränderungen, Herzveränderungen, Darmveränderungen, Nierenveränderungen, Milzveränderungen, Hautveränderungen, Liegebeulen/Bursitiden, Abszesse, Treibespuren."
  when-and-how-often = "Daten vom Schlachthof halbjährlich auswerten."
  which-and-how-many = "Alle Schlachtbefunde aller Sauen vom Schlacht- und Trimmband, gegebenenfalls aus der Lebendtierbeschau."
  how = "Auswertung der Schlachtbefunde des betrachteten Halbjahres hinsichtlich Verletzungen und Krankheiten. Am QS-System teilnehmende Tierhalter haben Zugang zur zentralen Befunddatenbank und können sich zu jeder Lieferpartie selbst die Einzeldaten der Schlachtbefunde herunterladen."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "Report"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Schlachtbefund"
          short-description = "Tiere mit Schlachtbefunden"
          long-description = "Tiere mit Schlachtbefunden"
          images = [ ]
        }
        Var "B" {
          name = "beurteilte Tiere"
          short-description = "Gesamtzahl beurteilter Tiere"
          long-description = "Gesamtzahl beurteilter Tiere"
          images = [ ]
        }
        //End-Var labelClass1
      }
      //End-Options
    }
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Schlachtbefunde"
      description = "Auswertung der Schlachtbefunde hinsichtlich Verletzungen und Krankheiten"
      expression = (A / B) * 100
    }
    // End-Formula

  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Nestbaumaterial"
  description = "Vor dem Abferkeln haben Sauen eine hohe Motivation, ein Nest zu bauen. Hierzu sollte geeignetes Material ab dem 112. Trächtigkeitstag zur Verfügung gestellt werden."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Vor dem Abferkeln haben Sauen eine hohe Motivation, ein Nest zu bauen. Hierzu sollte geeignetes Material wie Langstroh, Jutesäcke oder vergleichbare Materialien, die von der Sau ins Maul aufgenommen, am Boden bewegt oder hin und her getragen werden können, angeboten werden. Bei nicht vorhandenem oder nicht angenommenem Nestbaumaterial können die Tiere dieses Verhalten nicht ausüben, was zu Unruhe beim Tier, verlängerter Geburtsdauer und höheren Ferkelverlusten führen kann. Die Materialien zum Nestbau sollten der Sau ab dem 112. Trächtigkeitstag zur Verfügung gestellt und mindestens bis zum ersten Tag nach dem Abferkeln täglich erneuert bzw. beim Verbleiben in der Bucht auf Kotverschmutzung kontrolliert werden"
  when-and-how-often = "Halbjährlich, jeweils zur Mitte Sommer- und Winterhalbjahr, am Tag vor dem Abferkeln erheben und auswerten."
  which-and-how-many = "Abferkelstall. Jede Sau der neu eingestallten Gruppe."
  how = "Es wird erfasst, ob Nestbaumaterial wie Langstroh, Jutesäcke oder vergleichbare Materialien, die von der Sau ins Maul aufgenommen und am Boden bewegt oder hin und her getragen werden können, angeboten wird. „Geeignet“ ist das angebotene Nestmaterial, wenn es vorhanden ist, Gebrauchsspuren zeigt bzw. aufgebraucht wird und nicht mit Kot verschmutzt ist."
  branches = [
    "Abferkelstall"]
  sorting = 10
  Assessment {
    sample = "herd_size"
    default_type = "By_Indicator"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "geeignet"
          short-description = "Das angebotene Nestmaterial ist vorhanden, zeigt Gebrauchsspuren bzw. ist aufgebraucht und nicht mit Kot verschmutzt."
          long-description = "Das angebotene Nestmaterial ist vorhanden, zeigt Gebrauchsspuren bzw. ist aufgebraucht und nicht mit Kot verschmutzt."
          images = [ ]
        }
        Var "B" {
          name = "nicht geeignet"
          short-description = "Das angebotene Nestmaterial ist nicht vorhanden, zeigt keine Gebrauchsspuren und ist mit Kot verschmutzt."
          long-description = "Das angebotene Nestmaterial ist nicht vorhanden, zeigt keine Gebrauchsspuren und ist mit Kot verschmutzt."
          images = [ ]
        }
        //End-Var labelClass1
      }
      // End-Group Bonituren Gruppe 1
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Nestbaumaterial"
      description = "Anteil von Sauen mit geeigentem Nestbaumaterial"
      expression = (A / (A + B)) * 100
      Ranges {
        unit = "%"
        visual-lowerbound = 80
        visual-upperbound = 100
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier hat kein geeignetes Nestbaumaterial."
          lowerbound = 100
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 100
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere haben ungeeignetes Nestbaumaterial , sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 90
          upperbound = 100
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula

  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Anzeichen für Stereotypien"
  description = "Stereotypien sind Verhaltensweisen, die wiederholt in der immer gleichen Abfolge gezeigt werden und keine erkennbare Funktion haben. Sie sind ein Ausdruck von Frustration. Ein Hinweis auf diese Stereotypien ist in der Regel eine deutliche Schaumbildung am Maul der betroffenen Sauen."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Stereotypien sind Verhaltensweisen, die wiederholt in der immer gleichen Abfolge gezeigt werden und keine erkennbare Funktion haben. Sie sind ein Ausdruck von Frustration und weisen auf einen starken Beschäftigungsmangel hin, der häufig auf eine Unterversorgung mit strukturreichem Futter oder Beschäftigungsmaterial zurückzuführen ist. Leerkauen und Zungenrollen sind typische Stereotypien von Sauen. Beim Leerkauen zeigen Sauen Kaubewegungen ohne Futter oder anderes Material im Maul. Dieses Verhalten kann auch in Kombination mit Zähneknirschen und Zungenrollen auftreten. Beim Zungenrollen wird die Zunge wiederkehrend und über mehrere Sekunden anhaltend vor- und zurückbewegt oder auch eingerollt, ohne dass dabei Futter oder Material bearbeitet oder beleckt wird. Ein Hinweis auf diese Stereotypien ist in der Regel eine deutliche Schaumbildung am Maul der betroffenen Sauen. Schaum entsteht, wenn der angesammelte Speichel ständig von der Zunge bewegt und dabei mit Luft vermischt wird."
  when-and-how-often = "Halbjährlich, jeweils zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten."
  which-and-how-many = "Sauen im Deck-, Warte- und Abferkelstall"
  how = "Beurteilung der Tiere auf deutliche weiße Schaumbildung am Maul. Schaumbildung am Maul kann auch im Zusammenhang mit der Futteraufnahme entstehen. Dieser Indikator sollte deshalb nicht während oder unmittelbar nach der Fütterung erhoben werden."
  branches = [
