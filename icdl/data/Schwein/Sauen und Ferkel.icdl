Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Hautverletzungen (außer Vulva, Gesäuge und Schulterläsion)"
  description = "Verletzungen der Haut entstehen durch soziale Auseinandersetzungen beim Gruppieren der Tiere sowie durch gegenseitiges Verdrängen und sind ein Hinweis auf andauernden sozialen Stress."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Verletzungen der Haut entstehen durch soziale Auseinandersetzungen beim Gruppieren der Tiere (meist an Hals, Schulter oder Flanke) sowie durch gegenseitiges Verdrängen, z. B. vom Futterplatz. Auch hervorstehende Kanten der Aufstallung und des Bodens können zu Verletzungen führen. Hautverletzungen sind schmerzhaft. Wenn eine Woche oder länger nach der Neugruppierung sowohl frische als auch abheilende Verletzungen vorhanden sind, sind diese ein Hinweis auf andauernden sozialen Stress."
  when-and-how-often = "Halbjährlich, jeweils zur Mitte Sommer- und Winterhalbjahr, aber frühestens eine Woche nach Neugruppierungen erheben und auswerten."
  which-and-how-many = "Sauen im Deck-, Warte- und Abferkelstall"
  how = "Beurteilung einer je Tier zufällig ausgewählten Körperseite hinsichtlich frischer und verkrusteter Hautverletzungen. Ausgenommen sind Verletzungen an Vulva, Gesäuge und Schulterläsionen. Strichförmige und flächige Hautverletzungen können sowohl oberflächliche Kratzer als auch tiefergehende Wunden sein. Abheilende Verletzungen werden als eine durchgängige Verletzung gezählt, wenn die abgeheilten Zwischenräume jeweils nicht länger als 0,5 cm sind."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = ""
    default_type = "By_Animal"
    type_changeable = false
    presentation = "classification"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Bonitur 0"
          short-description = "< 4 strichförmige Verletzungen mit ≥ 5 cm Länge, und keine flächige Verletzung mit ≥ 2,5 cm Ø (2-Euro-Münze)"
          long-description = "< 4 strichförmige Verletzungen mit ≥ 5 cm Länge, und keine flächige Verletzung mit ≥ 2,5 cm Ø (2-Euro-Münze)"
          images = [
            "Hautverletzung1"]
          thumbnail = ""
        }
        Var "B" {
          name = "Bonitur 1"
          short-description = "4–15 strichförmige Verletzungen mit ≥ 5 cm Länge, und keine flächige Verletzung mit r ≥ 2,5 cm Ø (2-Euro-Münze)"
          long-description = "4–15 strichförmige Verletzungen mit ≥ 5 cm Länge, und keine flächige Verletzung mit r ≥ 2,5 cm Ø (2-Euro-Münze)"
          images = [
            "Hautverletzung2",
            "Hautverlezung3"]
          thumbnail = ""
        }
        Var "C" {
          name = "Bonitur 2"
          short-description = "> 15 strichförmige Verletzungen mit ≥ 5 cm Längeoder eine flächige Verletzung mit r ≥ 2,5 cm Ø (2-Euro-Münze)"
          long-description = "> 15 strichförmige Verletzungen mit ≥ 5 cm Längeoder eine flächige Verletzung mit r ≥ 2,5 cm Ø (2-Euro-Münze)"
          images = [
            "Hautverletzung4",
            "Hautverletzung5"]
          thumbnail = ""
        }
        //End-Var labelClass1
      }
      // End-Group Bonituren Gruppe 1
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "Hautverletzungen"
      description = "Anteil der Tiere mit starken Hautverletzungen"
      expression = (C / (A + B + C)) * 100
      Ranges {
        unit = "%"
        visual-lowerbound = 0
        visual-upperbound = 20
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier hat starke Hautverletzungen."
          lowerbound = 0
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 5
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere haben starke Hautverletzungen, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 10
          upperbound = 100
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      // End-Ranges
    }
    // End-Formula

  }
  // End-Evaluation
}
// End-Indicator indicatorLabel

Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Körperkondition"
  description = "Eine optimale Körperkondition ist wichtig für die Gesundheit und Aufzuchtleistung von Sauen."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Eine optimale Körperkondition ist wichtig für die Gesundheit und Aufzuchtleistung von Sauen. Unterkonditionierte Sauen weisen auf eine quantitativ oder qualitativ unzureichende Fütterung hin, d. h. entweder auf zu wenig Futter je Tier und Tag, zu wenig Fressplätze oder aber zu wenig Energie und/ oder Rohprotein. Außerdem kann Abmagerung ein Hinweis auf eine (chronische) Erkrankung oder einen eingeschränkten Zugang zum Futter sein, z. B. aufgrund von Lahmheit. Zu dünne Sauen nehmen schlechter auf, haben eine verminderte Milchleistung und entwickeln leichter Schulterläsionen. Eine Überkonditionierung hingegen weist auf eine Fütterung über den Bedarf hin, d. h. entweder auf zu viel Futter je Tier und Tag, nicht tierindividuelle Futtervorlage in der Gruppenhaltung oder zu viel Energie und zu wenig Rohfaser je kg Futter. Fette Sauen neigen zu Fundamentproblemen, einer längeren Geburtsdauer, erhöhten Totgeburtenraten und größeren tiergesundheitlichen Problemen nach der Geburt. Außerdem sind fette Sauen weniger beweglich und es besteht die Gefahr von erhöhten Erdrückungsverlusten neugeborener Ferkel."
  when-and-how-often = "Halbjährlich, jeweils zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten"
  which-and-how-many = "Sauen im Deck-, Warte- und Abferkelstall "
  how = "Beurteilung der Köperkondition mit dem Body Condition Score (BCS). Die Körperkondition sollte an stehenden Sauen beurteilt werden. Liegende Sauen im Abferkelstall lassen sich mit etwas Futter, das Sie zum Zeitpunkt der Beurteilung in den Trog geben, zum Aufstehen bewegen."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    Sample {
      // work-in-progress
      type = "complete|partial"
      partial-factor = 0.3
      // This key will only be of use if the sample-type is "partial"
    }
    default_type = "By_Indicator"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Bonitur 1"
          short-description = "Zu mager"
          long-description = "Sitzbein und Hüfthöcker stehen deutlich hervor. Schwanzansatz und Flanken sind eingefallen. Dornfortsätze der Wirbel stehen über den gesamten Rücken deutlich hervor. Einzelne Rippen sind klar zu erkennen."
          images = [
            "Körperkondition1"]
          thumbnail = ""
        }
        Var "B" {
          name = "Bonitur 2"
          short-description = "Mager"
          long-description = "Sitzbein- und Hüfthöcker sind leicht mit Gewebe bedeckt und können bei Auflegen des Handballens gefühlt werden. Schwanzansatz und Flanken sind leicht eingefallen. Nur einzelne Dornfortsätze der Wirbel und einzelne Rippen sind sichtbar."
          images = [
            "Körperkondition2"]
          thumbnail = ""
        }
        Var "C" {
          name = "Bonitur 3"
          short-description = "Gut"
          long-description = "Sitzbein- und Hüfthöcker sind mit Gewebe bedeckt, können aber mit leichtem Druck des Handballens ertastet werden. Die Dornfortsätze der Wirbel sind nur in Schulterhöhe gerade noch sichtbar. Der Schwanzansatz ist von Gewebe umgeben."
          images = [
            "Körperkondition3"]
          thumbnail = ""
        }
        Var "D" {
          name = "Bonitur 4"
          short-description = "Sehr gut"
          long-description = "Sitzbein- und Hüfthöcker sind sehr gut mit Gewebe bedeckt, können aber mit festem Druck eines Fingers ertastet werden. Becken und Rippen sind nicht sichtbar. Die Dornfortsätze der Rückenwirbel sind nicht sichtbar. Die Flanken sind voll. Am Schwanzansatz und an den Innenschenkeln sind leichte Fettfalten zu erkennen."
          images = [
            "Körperkondition4"]
          thumbnail = ""
        }
        Var "E" {
          name = "Bonitur 5"
          short-description = "Fett"
          long-description = "Becken, Rippen, Rücken- und Lendenwirbel lassen sich auch unter starkem Druck nicht ertasten. Der Schwanzansatz ist mit Fettfalten tief im Gewebe versunken. Starke Fettfalten an den Innenschenkeln."
          images = [
            "Körperkondition5"]
          thumbnail = ""
        }
        //End-Var labelClass1
      }
      // End-Group Bonituren Gruppe 1
    }
    //End-Options
  }
  //End-Assessment
  Evaluation {
    Formula {
      name = "zu magere Sauen"
      description = "Anteil der Sauen, die zu mager sind."
      expression = (A / (A + B + C + D + E)) * 100
      Ranges {
        unit = "%"
        visual-lowerbound = 0
        visual-upperbound = 5
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier ist zu mager."
          lowerbound = 0
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 0
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere sind zu mager, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 3
          upperbound = 100
        }
        Warningrange {
          tag = "Frühwarnbereich"
          description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
        }
      }
      Formula {
        name = "zu magere und magere Sauen"
        description = "Anteil der Sauen, die zu mager und mager sind."
        expression = "((A + B) / (A + B + C + D + E)) * 100"
      }
      Formula {
        name = "Fette Sauen"
        description = "Anteil der Sauen, die zu fett sind."
        expression = (E / (A + B + C + D + E)) * 100
        Ranges {
          unit = "%"
          visual-lowerbound = 0
          visual-upperbound = 10
          Targetrange {
            tag = "Zielbereich"
            description = "Kaum ein Tier ist zu fett."
            lowerbound = 0
            //Dezimalzahlen wären hier genauso möglich
            upperbound = 3
          }
          Alertrange {
            tag = "Alarmbereich"
            description = "Zu viele Tiere sind zu fett, sofortiger Handlungsbedarf ist nötig!"
            lowerbound = 6
            upperbound = 100
          }
          Warningrange {
            tag = "Frühwarnbereich"
            description = "Es läuft nicht optimal. Handlungsbedarf ist vorhanden"
          }
        }
        // End-Ranges
      }
      // End-Formula

    }
    // End-Evaluation
  }
}
// End-Indicator indicatorLabel

