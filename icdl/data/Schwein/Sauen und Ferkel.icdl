Indicator "indicatorLabel" {
  last-modified = "timestamp der letzten Änderung"
  name = "Anzeichen von Ektoparasiten "
  description = "Ektoparasiten sind Parasiten, die auf oder in der Haut der Schweine leben und sind für die Tiere nicht nur irritierend, sondern können auch schmerzhaft sein und zu erhöhter Unruhe und damit zu erhöhten Ferkelverlusten sowie verminderten Zunahmen führen."
  animal-category = "Sauen"
  video-ref = {
    "title1" = "link to video"
  }
  what-and-why = "Ektoparasiten sind Parasiten, die auf oder in der Haut der Schweine leben; insbesondere sind hier Räudemilben (Sarcoptes suis) und Läuse (Hämatopinus suis) relevant. Läuse und deren Eier sind direkt am Tier (Hinterhand bzw. Nacken) mit bloßem Auge erkennbar. Eine Infektion mit Räudemilben führt zunächst zu roten Punkten am ganzen Körper; chronische Infektionen bei Sauen äußern sich in graubraunen Krusten an den Unterbeinen, Schwanzansatz, Nacken und an den Ohren. In jedem Fall ist auffälliger Juckreiz zu beobachten. Ektoparasiten sind für die Tiere nicht nur irritierend, sondern können auch schmerzhaft sein und zu erhöhter Unruhe und damit zu erhöhten Ferkelverlusten sowie verminderten Zunahmen führen."
  when-and-how-often = "Halbjährlich, zur Mitte Sommer- und Winterhalbjahr, erheben und auswerten. Bester Zeitpunkt zur Erhebung der Hautirritationen ist nach dem Einstallen gewaschener Sauen in den Abferkelstall."
  which-and-how-many = "Sauen im Deck-, Warte- und Abferkelstall"
  how = "Beurteilung der Tiere aus ca. 1 m Entfernung hinsichtlich sichtbarer Parasiten wie Läuse, Läuseeier und Räude."
  branches = [
    "Abferkelstall",
    "Deckstall",
    "Wartestall"]
  sorting = 10
  Assessment {
    sample = ""
    default_type = "By_Indicator"
    type_changeable = false
    presentation = "counter"
    presentation_changeable = false
    Options {
      // Options is only used if the type is "classification"
      Group {
        name = "Bonituren Gruppe 1"
        Var "A" {
          name = "Läuse oder Eier"
          short-description = "Sichtbare Parasiten"
          long-description = "Mit bloßem Auge sichtbare Parasiten wie Läuse-und/oder deren Eier an den Borsten klebend, zusätzlich oft intensives Scheuern an Gegenständen oder Kratzen mit Beinen"
          images = [
            "Ektoparasiten1",
            "Ektoparasiten2"]
          thumbail = "Ektoparasiten1"
        }
        Var "B" {
          name = "Beginnende Räude"
          short-description = "Viele rote Punkte"
          long-description = "Hautirritationen sind wie viele rote Punkte über den Körper verteilt, zusätzlich intensives Scheuern an Gegenständen oder Kratzen mit Beinen"
          images = [
            "Ektoparasiten3"]
          thumbail = "Ektoparasiten3"
        }
        Var "C" {
          name = "Räude"
          short-description = "Graubraune Krusten"
