        unit = "%"
        visual-lowerbound = 0
        visual-upperbound = 20
        Targetrange {
          tag = "Zielbereich"
          description = "Kaum ein Tier hat starke Kotverschmutzungen."
          lowerbound = 0
          //Dezimalzahlen wären hier genauso möglich
          upperbound = 5
        }
        Alertrange {
          tag = "Alarmbereich"
          description = "Zu viele Tiere haben starke Kotverschmutzungen, sofortiger Handlungsbedarf ist nötig!"
          lowerbound = 10
          upperbound = 100
